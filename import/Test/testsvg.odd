<?xml version="1.0" encoding="UTF-8"?>
<TEI xml:lang="en"  xmlns="http://www.tei-c.org/ns/1.0"
 xmlns:rng="http://relaxng.org/ns/structure/1.0"
 n="testsvg">
   <teiHeader>
      <fileDesc>
         <titleStmt>
            <title>TEI with SVG and MathML</title>
            <author>Sebastian Rahtz</author>
         </titleStmt>
         <publicationStmt>
	   <p> </p>
	 </publicationStmt>
         <sourceDesc>
            <p>authored from scratch</p>
         </sourceDesc>
      </fileDesc>
   </teiHeader>
<text>
<body>
  <p>
    <schemaSpec ident="testsvg" start="TEI">
      <moduleRef url="svg-main.rng"/>
      <moduleRef key="header"/>
      <moduleRef key="core"/>
      <moduleRef key="tei"/>
      <moduleRef key="textstructure"/>
      <moduleRef key="linking"/>
      <moduleRef key="figures"/>
      <elementSpec module="figures" ident="figure" mode="change">
	<content>
	  <rng:group>
	    <rng:optional>
	      <rng:ref name="head"/>
	      <rng:zeroOrMore>
		<rng:ref name="tei.Incl"/>
	      </rng:zeroOrMore>
	    </rng:optional>
	    <rng:ref name="svg.svg"/>
	    <rng:optional>
	      <rng:ref name="figDesc"/>
	    </rng:optional>
	  </rng:group>
	</content>
      </elementSpec>
    </schemaSpec>
  </p>
</body>
</text>
</TEI>



