<?xml version="1.0" encoding="UTF-8"?>
<TEI xml:lang="en"  xmlns="http://www.tei-c.org/ns/1.0"
 xmlns:rng="http://relaxng.org/ns/structure/1.0"
 n="testfand4">
   <teiHeader>
      <fileDesc>
         <titleStmt>
            <title>TEI with anti-FAND setup</title>
            <author>Sebastian Rahtz</author>
         </titleStmt>
         <publicationStmt>
	   <p> </p>
	 </publicationStmt>
         <sourceDesc>
            <p>authored from scratch</p>
         </sourceDesc>
      </fileDesc>
   </teiHeader>
<text>
<body>
<divGen type="toc"/>
<div>
<head>My old-style Schema</head>
<p>This schema restores div0.</p>
  <p>Here is the schema:</p>
    <schemaSpec ident="testfand4" start="TEI">
      <moduleRef key="tei"/>
      <moduleRef key="core"/>
      <moduleRef key="header"/>
      <moduleRef key="textstructure"/>
      <moduleRef key="msdescription"/>

      <elementSpec ident="div0" module="textstructure" mode="add">
	<classes>
	  <memberOf key="model.div1Like"/>
	</classes>
	<content>
	  <rng:group xmlns:rng="http://relaxng.org/ns/structure/1.0">
	    <rng:zeroOrMore>
	      <rng:choice>
		<rng:ref name="model.divWrapper"/>
		<rng:ref name="model.global"/>
	      </rng:choice>
	    </rng:zeroOrMore>
	    <rng:choice>
	      <rng:oneOrMore>
		<rng:choice>
		  <rng:ref name="model.div1Like"/>
		  <rng:ref name="model.divGenLike"/>
		</rng:choice>
		<rng:zeroOrMore>
		  <rng:ref name="model.global"/>
		</rng:zeroOrMore>
	      </rng:oneOrMore>
	      <rng:group>
		<rng:oneOrMore>
		  <rng:group><rng:ref name="model.common"/></rng:group>
		  <rng:zeroOrMore>
		    <rng:ref name="model.global"/>
		  </rng:zeroOrMore>
		</rng:oneOrMore>
		<rng:zeroOrMore>
		  <rng:choice>
		    <rng:ref name="model.div1Like"/>
		    <rng:ref name="model.divGenLike"/>
		  </rng:choice>
		  <rng:zeroOrMore>
		    <rng:ref name="model.global"/>
		  </rng:zeroOrMore>
		</rng:zeroOrMore>
	      </rng:group>
	    </rng:choice>
	    <rng:zeroOrMore>
	      <rng:choice>
		<rng:ref name="model.divWrapper"/>
		<rng:ref name="model.divWrapper.bottom"/>
	      </rng:choice>
	      <rng:zeroOrMore>
		<rng:ref name="model.global"/>
	      </rng:zeroOrMore>
	    </rng:zeroOrMore>
	  </rng:group>
	</content>

      </elementSpec>

    </schemaSpec>
</div>
</body>
</text>
</TEI>

