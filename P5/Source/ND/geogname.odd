<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="namesdates" xml:id="GEOGNAME" usage="opt" ident="geogName">
  <equiv/>
  <gloss>geographical name</gloss>
  <desc>a name associated with some geographical feature such as
  Windrush Valley or Mount Sinai.</desc>
  <classes>
    <memberOf key="model.pPart.data"/>
    <memberOf key="att.naming"/>
    <memberOf key="model.nameLike"/>
  </classes>
  <content>
    <rng:zeroOrMore xmlns:rng="http://relaxng.org/ns/structure/1.0">
      <rng:choice>
        <rng:text/>
<rng:ref name="model.gLike"/>
        <rng:ref name="geog"/>
        <rng:ref name="model.nameLike"/>
        <rng:ref name="model.global"/>
      </rng:choice>
    </rng:zeroOrMore>
  </content>
  <attList>
    <attDef ident="type" usage="mwa">
      <equiv/>
      <desc>provides more culture- linguistic- or application-
      specific information used to categorize this name
      component.</desc>
      <datatype>
	<rng:ref xmlns:rng="http://relaxng.org/ns/structure/1.0" name="data.enumerated"/>
      </datatype>
      <valDesc>one of a set of codes defined for the application.</valDesc>
    </attDef>
  </attList>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples">      <geogName>
        <geog>Mount</geog>
        <name>Sinai</name>
      </geogName>
    </egXML>
  </exemplum>
  <remarks/>
  <listRef>
    <ptr target="#NDPLAC"/>
  </listRef>
</elementSpec>
