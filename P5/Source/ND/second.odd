<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="namesdates" xml:id="SECOND" usage="opt" ident="second">
<equiv/>
<gloss>second</gloss>
<desc>the second component of a structured time-expression.</desc>
  <classes>
    <memberOf key="model.datePart"/>
    <memberOf key="att.datePart"/>
  </classes>
  <content>
    <rng:ref name="macro.xtext" xmlns:rng="http://relaxng.org/ns/structure/1.0"/>
  </content>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples">At the third stroke the time sponsored by Accurist
  will be 
  <timeStruct value="18:21:30">    
      <hour>six</hour>
      <minute>twenty-one</minute> and 
    <second>thirty</second> seconds
  </timeStruct></egXML>
  </exemplum>
  <remarks/>
  <listRef>
    <ptr target="#NDDATE"/>
  </listRef>
</elementSpec>
