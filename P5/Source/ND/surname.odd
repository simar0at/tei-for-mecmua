<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="namesdates" xml:id="SURNAME" usage="opt" ident="surname">
<equiv/>
<gloss>surname</gloss>
<desc>contains a family (inherited) name, as opposed to a given,
baptismal, or nick name.</desc>
  <classes>
    <memberOf key="model.persNamePart"/>
    <memberOf key="att.personal"/>
  </classes>
  <content>
    <rng:ref xmlns:rng="http://relaxng.org/ns/structure/1.0" name="macro.phraseSeq"/>
  </content>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples">      <surname type="combine">St John Stevas</surname>
    </egXML>
  </exemplum>
  <remarks/>
  <listRef>
    <ptr target="#NDPER"/>
  </listRef>
</elementSpec>
