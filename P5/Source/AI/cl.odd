<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="analysis" xml:id="CL" usage="opt" ident="cl">
<equiv/>
<gloss>clause</gloss>
<desc>represents a grammatical clause.</desc>
  <classes>
    <memberOf key="model.segLike"/>
    <memberOf key="att.segLike"/>
  </classes>
  <content>
    <rng:ref xmlns:rng="http://relaxng.org/ns/structure/1.0" name="macro.phraseSeq"/>
  </content>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples"><cl type="relative" function="clause modifier">Which frightened
 both the heroes so,<cl>They quite forgot their quarrel.</cl></cl></egXML>
  </exemplum>
  <remarks/>
  <listRef>
    <ptr target="#AILC"/>
  </listRef>
</elementSpec>
