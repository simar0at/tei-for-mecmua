<classSpec xmlns="http://www.tei-c.org/ns/1.0" module="tei" type="model" ident="model.timeLike">
  <gloss>times and time ranges</gloss>
  <desc>groups elements containing a time specifications.</desc>
  <classes>
    <memberOf key="model.pPart.data"/>
  </classes>
  <remarks>
    <p>This class allows certain content models to allow either a
    single time or a time-range element.</p>
  </remarks>
  <listRef>
    <ptr target="#CONADA"/>
    <ptr target="#NDDATE"/>
  </listRef>
</classSpec>
