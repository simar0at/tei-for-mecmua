<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="msdescription" xml:id="MATERIAL" ident="material">
  <equiv/>
  <gloss/>
  <desc xml:id="material.desc">contains a word or phrase describing the
  material of which  a manuscript (or part of a manuscript) is composed.
  </desc><desc version="2006-10-15" xml:lang="es">contiene una frase que describe el material de cual está compuesta cualquier parte de un manuscrito o encuadernación.</desc>
  <classes>
    <memberOf key="model.pPart.msdesc"/>
  </classes>
  <content>
<!--<!ELEMENT material  - -  (%phrase.seq;)>-->
    <rng:ref xmlns:rng="http://relaxng.org/ns/structure/1.0" name="macro.phraseSeq"/>
  </content>
  
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples" xmlns:rng="http://relaxng.org/ns/structure/1.0">&lt;physDesc&gt;&lt;p&gt;
&lt;material&gt;Parchment&lt;/material&gt; leaves with a
&lt;material&gt;sharkskin&lt;/material&gt; binding.&lt;/p&gt;&lt;/physDesc&gt;</egXML>
  </exemplum>
  <listRef>
    <ptr target="#msdates"/>
  </listRef>
</elementSpec>
