<elementSpec xmlns="http://www.tei-c.org/ns/1.0" xmlns:rng="http://relaxng.org/ns/structure/1.0" module="msdescription" xml:id="MSIDENTIFIER" ident="msIdentifier">
<equiv/>
<gloss/>
<desc xml:id="msidentifier.desc">contains the information required to identify
the  manuscript being described. </desc>
  <classes>
    <memberOf key="model.biblPart"/>
  </classes>
  <content>
<rng:group>
<rng:choice>
<rng:group>
