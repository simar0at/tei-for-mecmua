<classSpec xmlns="http://www.tei-c.org/ns/1.0" module="tei" xml:id="CLDATE" type="model" ident="model.dateLike">
  <gloss>dates and times</gloss>
  <gloss version="2007-05-04" xml:lang="es">fechas y horas</gloss>
  <gloss version="2007-05-13" xml:lang="it">dates and date ranges</gloss>
  <gloss version="2007-01-21" xml:lang="it">date e periodi compresi tra date</gloss>
  <desc>groups elements containing temporal expressions.</desc>
  <desc version="2007-05-04" xml:lang="es">agrupa elementos que contienen expresiones temporales.</desc>
  <desc version="2007-05-13" xml:lang="it">groups elements containing a date specifications.</desc>
  <desc version="2007-01-21" xml:lang="it">raggruppa elementi che contengono indicazioni relative a date</desc>
  <classes>
    <memberOf key="model.pPart.data"/>
    <memberOf key="model.recordingPart"/>
  </classes>
  <remarks>
    <p>This class allows certain content models to permit a temporal expression encoded with either a
     <gi>date</gi> or a <gi>time</gi> element.</p>
  </remarks>
  <listRef>
    <ptr target="#CONADA"/>
    <ptr target="#NDDATE"/>
  </listRef>
</classSpec>
