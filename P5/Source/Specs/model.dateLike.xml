<classSpec xmlns="http://www.tei-c.org/ns/1.0" module="tei" xml:id="CLDATE" type="model" ident="model.dateLike">
  <gloss>dates and times</gloss>
  <desc>groups elements containing temporal expressions.</desc>
  <classes>
    <memberOf key="model.pPart.data"/>
    <memberOf key="model.recordingPart"/>
  </classes>
  <remarks>
    <p>This class allows certain content models to permit a temporal expression encoded with either a
     <gi>date</gi> or a <gi>time</gi> element.</p>
  </remarks>
  <listRef>
    <ptr target="#CONADA"/>
    <ptr target="#NDDATE"/>
  </listRef>
</classSpec>
