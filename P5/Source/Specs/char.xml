<elementSpec xmlns="http://www.tei-c.org/ns/1.0" xmlns:rng="http://relaxng.org/ns/structure/1.0" module="gaiji" xml:id="CHAR" ident="char">
<equiv/>
<gloss>character</gloss>
<desc>provides descriptive information about a character<!-- which is not
otherwise available in the document character set-->. </desc><desc version="2006-10-15" xml:lang="es">proporciona información descriptiva sobre un carácter. </desc>
<classes/>
<content>
<rng:group>
      <rng:ref name="charName"/>
      <rng:ref name="macro.glossSeq"/>
      <rng:zeroOrMore>
        <rng:ref name="charProp"/>
      </rng:zeroOrMore>
      <rng:zeroOrMore>
        <rng:ref name="mapping"/>
      </rng:zeroOrMore>
      <rng:zeroOrMore>
        <rng:ref name="model.graphicLike"/>
      </rng:zeroOrMore>
      <rng:optional>
        <rng:ref name="model.noteLike"/>
      </rng:optional>
</rng:group>
</content>
  
<exemplum>
 <egXML xmlns="http://www.tei-c.org/ns/Examples">
 <char xml:id="circledU4EBA">
  <charName>CIRCLED IDEOGRAPH 4EBA</charName>
  <charProp>
    <unicodeName>character-decomposition-mapping</unicodeName>
    <value>circle</value>
  </charProp>
  <charProp>
   <localName>daikanwa</localName>
   <value>36</value>
  </charProp>
  <mapping type="standard">
   <g ref="#U4EBA">人</g>
  </mapping>
 </char>
</egXML>
</exemplum>
</elementSpec>
