<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="namesdates" xml:id="PLACNAME" usage="opt" ident="placeName">
  <equiv/>
<!--gloss>place name</gloss-->
  <gloss version="2006-10-28" xml:lang="ja">場所名</gloss>
  <gloss version="2007-08-25" xml:lang="it">place name</gloss>
  <gloss version="2007-01-21" xml:lang="it">nome di luogo</gloss>
  <desc>contains an absolute or relative place name. </desc>
  <desc version="2007-01-21" xml:lang="it">contiene l'indicazione assoluta o relativa di un nome di luogo</desc>
  <desc version="2007-05-04" xml:lang="es">contiene la indicación absoluta o relativa de un nombre de lugar.</desc>
  <desc version="2006-10-28" xml:lang="ja"> 絶対的・相対的に場所を示す．</desc>
  <classes>
    <memberOf key="model.placeStateLike"/>
    <memberOf key="model.placeNamePart"/>
    <memberOf key="att.naming"/>
    <memberOf key="att.typed"/>
    <memberOf key="att.datable"/>
    <memberOf key="att.editLike"/>
  </classes>
  <content>
    <rng:ref xmlns:rng="http://relaxng.org/ns/structure/1.0" name="macro.phraseSeq"/>
  </content>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples">
      <placeName>
        <settlement>Rochester</settlement>
        <region>New York</region>
      </placeName>
    </egXML>
  </exemplum>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples">
      <placeName>
        <geogName>Arrochar Alps</geogName>
        <region>Argylshire</region>
      </placeName>
    </egXML>
  </exemplum>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples">
      <placeName>
        <measure>10 miles</measure>
        <offset>Northeast of</offset>
        <settlement>Attica</settlement>
      </placeName>
    </egXML>
  </exemplum>
  <listRef>
    <ptr target="#NDPLAC"/>
  </listRef>
</elementSpec>
