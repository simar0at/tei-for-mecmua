<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="namesdates" xml:id="MINUTE" usage="opt" ident="minute">
<equiv/>
<gloss>minute</gloss>
<desc>the minute component of a structured time-expression.</desc>
  <classes>
    <memberOf key="model.datePart"/>
    <memberOf key="att.datePart"/>
  </classes>
  <content>
    <rng:ref name="macro.xtext" xmlns:rng="http://relaxng.org/ns/structure/1.0"/>
  </content>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples">The train leaves for Boston at 
    <time value="13:45:00">
      <hour>13</hour>:<minute>45</minute>
   </time></egXML>
  </exemplum>
  <remarks/>
  <listRef>
    <ptr target="#NDDATE"/>
  </listRef>
</elementSpec>
