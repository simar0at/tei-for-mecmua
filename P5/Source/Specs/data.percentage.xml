<macroSpec xmlns="http://www.tei-c.org/ns/1.0" xmlns:rng="http://relaxng.org/ns/structure/1.0" module="tei" type="dt" ident="data.percentage">
  <desc>defines attribute values used to express a percentage value.</desc>
  <content>
        <data xmlns="http://relaxng.org/ns/structure/1.0" type="nonNegativeInteger">
          <param name="maxInclusive">100</param>
        </data>
  </content>
  <remarks>
    <p>Any non-negative integer value  less than 100.</p>
  </remarks>
</macroSpec>
