<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="core" xml:id="IMPRINT" usage="opt" ident="imprint">
  <equiv/>
  <gloss/>
  <desc>groups information relating to the publication or distribution
  of a bibliographic item.</desc><desc version="2006-10-28" xml:lang="ja">書誌情報の対象となるものの，出版に関する情報．</desc>
  <classes/>
  <content xmlns:rng="http://relaxng.org/ns/structure/1.0" xmlns="http://www.tei-c.org/ns/1.0">
    <rng:oneOrMore>
      <rng:choice>
	<rng:group>  <!--just here to so odd2dtd puts parens into DTD-->
	  <rng:ref name="model.imprintPart"/>
	</rng:group>
	<rng:group>  <!--just here to so odd2dtd puts parens into DTD-->
	  <rng:ref name="model.dateLike"/>
	</rng:group>
      </rng:choice>
      <rng:zeroOrMore>
        <rng:ref name="model.global"/>
      </rng:zeroOrMore>
    </rng:oneOrMore>
  </content>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples">      <imprint>
        <pubPlace>Oxford</pubPlace>
        <publisher>Clarendon Press</publisher>
        <date>1987</date>
      </imprint>
    </egXML>
  </exemplum>
  <remarks/>
  <listRef>
    <ptr target="#COBICOI"/>
  </listRef>
</elementSpec>
