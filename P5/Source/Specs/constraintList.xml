<elementSpec 
    xmlns:s="http://www.ascc.net/xml/schematron" 
    xmlns="http://www.tei-c.org/ns/1.0" xmlns:rng="http://relaxng.org/ns/structure/1.0" module="tagdocs" ident="constraintList">
  <desc>contains a collection of constraints on use of the schema</desc>
  <content>
    <rng:group>
     <rng:oneOrMore>
       <rng:ref name="constraint"/>
     </rng:oneOrMore>
    </rng:group>
  </content>
  <attList>
    <attDef ident="scheme">
      <equiv/>
      <desc>supplies the name of the language in which the constraints
      are defined</desc>
      <valList type="closed">
        <valItem ident="schematron">
          <gloss>Schematron</gloss>
	</valItem>
        <valItem ident="xsl">
          <gloss>XSLT</gloss>
	</valItem>
        <valItem ident="private">
          <gloss>private constraint language</gloss>
        </valItem>
      </valList>
    </attDef>
  </attList>
  <listRef>
    <ptr target="#TDTAG"/>
  </listRef>
</elementSpec>
