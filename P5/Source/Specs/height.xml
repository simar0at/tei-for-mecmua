  <content>
    <rng:ref xmlns:rng="http://relaxng.org/ns/structure/1.0" name="macro.xtext"/>
  </content>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples" xmlns:rng="http://relaxng.org/ns/structure/1.0">
      <height unit="in" quantity="7"/>
