<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="msdescription" xml:id="ADDITIONAL" ident="additional">
<equiv/>
<gloss/>
<desc xml:id="additional.desc">groups additional information, combining
bibliographic information about a manuscript, or surrogate copies of
it with  curatorial or administrative information. </desc><desc version="2006-10-15" xml:lang="es">agrupa información adicional relacionada
con la bibliografía moderna de un manuscrito, su estado de conservación
actual, y otros materiales asociados.
		</desc>
  <classes/>
  <content>
<!--<!ELEMENT additional  - -  (adminInfo?, surrogates?, listBibl?)>
-->
<rng:group xmlns:rng="http://relaxng.org/ns/structure/1.0">
    <rng:optional>
      <rng:ref name="adminInfo"/>
    </rng:optional>
    <rng:optional>
      <rng:ref name="surrogates"/>
    </rng:optional>
    <rng:optional>
      <rng:ref name="listBibl"/>
    </rng:optional>
</rng:group>
  </content>
  
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples"><additional>
<adminInfo>
  <recordHist>
   <!-- record history here -->
  </recordHist>
<custodialHist>
  <!-- custodial history here -->
</custodialHist> 
</adminInfo>
<surrogates>
  <!-- information about surrogates here -->
</surrogates>
<listBibl>
  <!-- full bibliography here -->
</listBibl>
</additional></egXML>
  </exemplum>
  <listRef>
    <ptr target="#msad"/>
  </listRef>
</elementSpec>
