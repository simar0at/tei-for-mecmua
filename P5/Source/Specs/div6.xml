<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="textstructure" xml:id="DIV6" usage="rwa" ident="div6">
<equiv/>
<gloss>level-6 text division</gloss><gloss version="2006-10-28" xml:lang="ja">レベル6テキスト区分</gloss>
<desc>contains a sixth-level subdivision of the front, body, or back of a
 text.</desc><desc version="2006-10-28" xml:lang="ja"> レベル5より小さい部分を示す．</desc><desc version="2006-10-15" xml:lang="es">contiene una subdivisión de sexto nivel de los preliminares, cuerpo, o anexos de un texto.</desc>
  <classes>
    <memberOf key="att.divLike"/>
    <memberOf key="att.declaring"/>
    <memberOf key="model.div6Like"/>
  </classes>
  <content>
    <rng:group xmlns:rng="http://relaxng.org/ns/structure/1.0">
      <rng:zeroOrMore>
        <rng:choice>
          <rng:ref name="model.divWrapper"/>
          <rng:ref name="model.global"/>
        </rng:choice>
      </rng:zeroOrMore>
<rng:optional>
      <rng:choice>
        <rng:oneOrMore>
          <rng:choice>
            <rng:ref name="model.div7Like"/>
            <rng:ref name="model.divGenLike"/>
          </rng:choice>
          <rng:zeroOrMore>
            <rng:ref name="model.global"/>
          </rng:zeroOrMore>
        </rng:oneOrMore>
        <rng:group>
          <rng:oneOrMore>
            <rng:ref name="macro.component"/>
            <rng:zeroOrMore>
              <rng:ref name="model.global"/>
            </rng:zeroOrMore>
          </rng:oneOrMore>
          <rng:zeroOrMore>
            <rng:choice>
              <rng:ref name="model.div7Like"/>
              <rng:ref name="model.divGenLike"/>
            </rng:choice>
            <rng:zeroOrMore>
              <rng:ref name="model.global"/>
            </rng:zeroOrMore>
          </rng:zeroOrMore>
        </rng:group>
      </rng:choice>
      <rng:zeroOrMore>
	<rng:choice>
	  <rng:ref name="model.divWrapper"/>
	  <rng:ref name="model.divWrapper.bottom"/>
	</rng:choice>
	<rng:zeroOrMore>
	  <rng:ref name="model.global"/>
	</rng:zeroOrMore>
      </rng:zeroOrMore>
</rng:optional>
    </rng:group>
  </content>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples">    </egXML>
  </exemplum>
  <remarks>
    <p rend="dataDesc">any sequence of low-level structural elements, possibly grouped
into lower subdivisions.</p>
  </remarks>
  <listRef>
    <ptr target="#DSDIV2"/>
  </listRef>
</elementSpec>
