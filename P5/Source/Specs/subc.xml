<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="dictionaries" xml:id="SUBC" usage="opt"
    ident="subc">
    <equiv/>
    <gloss>subcategorization</gloss>
    <gloss version="2006-10-28" xml:lang="ja">下位範疇</gloss>
    <desc>contains subcategorization information (transitive/intransitive, countable/non-countable,
        etc.)</desc>
    <desc version="2006-10-28" xml:lang="ja"> 下位範疇情報を示す．例えば，自動詞・他動詞，可算・非可 算など．</desc>
    <classes>
        <memberOf key="model.entryParts"/>
        <memberOf key="model.gramPart"/>
        <memberOf key="att.lexicographic"/>
    </classes>
    <content>
        <rng:ref xmlns:rng="http://relaxng.org/ns/structure/1.0" name="macro.paraContent"/>
    </content>
    <exemplum>
        <egXML xmlns="http://www.tei-c.org/ns/Examples">
            <entry>
                <form>
                    <orth>médire</orth>
                </form>
                <gramGrp>
                    <subc>t ind</subc>
                </gramGrp>
            </entry>
        </egXML>
    </exemplum>
    <remarks>
        <p/>
    </remarks>
    <listRef>
        <ptr target="#DITPGR"/>
    </listRef>
</elementSpec>

