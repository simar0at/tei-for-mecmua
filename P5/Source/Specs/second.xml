<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="namesdates" xml:id="SECOND" usage="opt" ident="second">
<equiv/>
<gloss>second</gloss><gloss version="2006-10-28" xml:lang="ja">秒</gloss>
<desc>the second component of a structured time-expression.</desc><desc version="2006-10-28" xml:lang="ja"> 時間表現のうち，秒の値を示す．</desc><desc version="2006-10-15" xml:lang="es">el componente segundos en una expresión de tiempo estructurada.</desc>
  <classes>
    <memberOf key="model.datePart"/>
    <memberOf key="att.datePart"/>
  </classes>
  <content>
    <rng:ref xmlns:rng="http://relaxng.org/ns/structure/1.0" name="macro.xtext"/>
  </content>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples">At the third stroke the time sponsored by Accurist
  will be 
  <time value="18:21:30">    
      <hour>six</hour>
      <minute>twenty-one</minute> and 
    <second>thirty</second> seconds
  </time></egXML>
  </exemplum>
  <remarks/>
  <listRef>
    <ptr target="#NDDATE"/>
  </listRef>
</elementSpec>
