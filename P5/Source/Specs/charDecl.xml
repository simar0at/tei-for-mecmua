<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="gaiji"  ident="charDecl">
  <equiv/>
  <gloss>character description</gloss>
  <gloss version="2007-01-21" xml:lang="it">descrizione del carattere</gloss>
  <gloss version="2007-05-04" xml:lang="es">descripción del carácter</gloss>
  <gloss version="2006-10-28" xml:lang="ja">文字解説</gloss>
  <desc>provides descriptive information about characters or
glyphs. </desc>
  <desc version="2007-01-21" xml:lang="it">fornisce informazioni descrittive su un caratteri o glifi.</desc>
  <desc version="2007-05-04" xml:lang="es">proporciona información descriptiva sobre los carácteres o pictogramas.</desc>
  <desc version="2006-10-28" xml:lang="ja"> 文字やグリフに関する解説を示す．</desc>
  <classes>
    <memberOf key="model.encodingPart"/>
  </classes>
  <content>
<!--(desc?, (char|glyph)+)-->
    <rng:group xmlns:rng="http://relaxng.org/ns/structure/1.0">
      <rng:optional>
        <rng:ref name="desc"/>
      </rng:optional>
      <rng:oneOrMore>
        <rng:choice>
          <rng:ref name="char"/>
          <rng:ref name="glyph"/>
        </rng:choice>
      </rng:oneOrMore>
    </rng:group>
  </content>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples"/>
  </exemplum>
</elementSpec>
