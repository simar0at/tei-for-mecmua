<?xml version="1.0" encoding="utf-8"?>
<elementSpec xmlns="http://www.tei-c.org/ns/1.0" xmlns:rng="http://relaxng.org/ns/structure/1.0" module="namesdates" usage="opt" ident="nym">
  <gloss>canonical name</gloss>
  <desc>contains the definition for a canonical  name or namepart of
  any kind</desc>
  <desc version="2007-05-04" xml:lang="es">contiene la definición para un nombre canónico o parte de nombre de cualquier tipo.</desc>
  <classes>
    <memberOf key="att.typed"/>
  </classes>
  <content>
    <rng:group>
      <rng:group>
        <rng:zeroOrMore>
          <rng:ref name="model.entryParts"/>
        </rng:zeroOrMore>
      </rng:group>
      <rng:group>
        <rng:zeroOrMore>
          <rng:ref name="model.pLike"/>
        </rng:zeroOrMore>
      </rng:group>
      <rng:group>
        <rng:zeroOrMore>
          <rng:ref name="nym"/>
        </rng:zeroOrMore>
      </rng:group>
    </rng:group>
  </content>
  <attList>
    <attDef ident="parts">
      <desc>points to constituent nyms </desc>
      <desc version="2007-05-04" xml:lang="es">señala un componente nyms.</desc>
      <datatype minOccurs="1" maxOccurs="100">
        <rng:ref name="data.pointer"/>
      </datatype>
    </attDef>
  </attList>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples">
      <nym xml:id="J452">
        <form>
          <orth xml:lang="en-US">Ian</orth>
          <orth xml:lang="en-x-Scots">Iain</orth>
        </form>
      </nym>
    </egXML>
  </exemplum>
  <remarks>
    <p/>
  </remarks>
  <listRef>
    <ptr target="#NDNYM" type="div2"/>
  </listRef>
</elementSpec>
