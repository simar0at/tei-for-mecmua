<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="core" xml:id="ADDRLINE" usage="opt" ident="addrLine">
<equiv/>
<gloss/>
<desc>contains one line of a postal or other address.</desc><desc version="2006-10-15" xml:lang="es">contiene una línea de una dirección postal o de otro tipo.</desc>
  <classes>
    <memberOf key="model.addrPart"/>
  </classes>

  <content>
    <rng:ref xmlns:rng="http://relaxng.org/ns/structure/1.0" name="macro.phraseSeq"/>
  </content>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples">      <address>
        <addrLine>Computing Center, MC 135</addrLine>
        <addrLine>P.O. Box 6998</addrLine>
        <addrLine>Chicago, IL</addrLine>
        <addrLine>60680 USA</addrLine>
      </address>
    </egXML>
  </exemplum>
  <remarks>
    <p>Addresses may be encoded either as a sequence of 
lines, or using any sequence of address component elements.</p>
  </remarks>
  <listRef>
    <ptr target="#CONAAD"/>
    <ptr target="#HD24"/>
    <ptr target="#COBICOI"/>
  </listRef>
</elementSpec>
