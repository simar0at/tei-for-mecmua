<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="dictionaries" xml:id="SUPENTRY" usage="rwa" ident="superEntry">
  <equiv/>
  <gloss/>
  <desc>groups successive entries for a set of homographs.</desc>
  <desc version="2007-01-21" xml:lang="it">raggruppa voci successive per una serie di omografi.</desc>
  <desc version="2007-05-04" xml:lang="es">agrupa las entradas sucesivas para una serie de homógrafos.</desc>
  <desc version="2006-10-28" xml:lang="ja">同形異音集合をなす要素をまとめる．</desc>
  <classes>
    <memberOf key="model.entryLike"/>
    <memberOf key="att.entryLike"/>
    <memberOf key="model.entryParts"/>
  </classes>
  <content>
    <rng:choice xmlns:rng="http://relaxng.org/ns/structure/1.0">
      <rng:group>
        <rng:optional>
          <rng:ref name="form"/>
        </rng:optional>
        <rng:oneOrMore>
          <rng:ref name="entry"/>
        </rng:oneOrMore>
      </rng:group>
      <rng:ref name="dictScrap"/>
    </rng:choice>
  </content>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples">
      <superEntry>
        <form>
          <orth>abandon</orth>
          <hyph>a|ban|don</hyph>
          <pron>@"b&amp;nd@n</pron>
        </form>
        <entry n="1">
          <gramGrp>
            <pos>v</pos>
            <subc>T1</subc>
          </gramGrp>
          <sense n="1">
            <def>to leave completely and for ever ... </def>
          </sense>
          <sense n="2"/>
        </entry>
        <entry n="2">
          <gramGrp>
            <pos>n</pos>
            <subc>U</subc>
          </gramGrp>
          <def>the state when one's feelings and actions are uncontrolled; freedom from
                        control</def>
        </entry>
      </superEntry>
    </egXML>
  </exemplum>
  <remarks>
    <p/>
  </remarks>
  <listRef>
    <ptr target="#DIBO"/>
  </listRef>
</elementSpec>
