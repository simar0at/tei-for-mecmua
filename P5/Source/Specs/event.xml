<?xml version="1.0" encoding="utf-8"?>
<elementSpec xmlns="http://www.tei-c.org/ns/1.0" xmlns:rng="http://relaxng.org/ns/structure/1.0" module="namesdates" ident="event">
  <desc>contains a description of a particular event of significance in
the life of  a person.</desc>
  <desc version="2007-05-04" xml:lang="es">contiene la descripción de un evento significativo en la vida de una persona.</desc>
  <desc version="2007-01-21" xml:lang="it">contiene la descrizione di un evento significativo nella vita di una persona</desc>
  <desc version="2006-10-28" xml:lang="ja">人物の人生における特定の事態についてします．</desc>
  <classes>
    <memberOf key="att.datable"/>
    <memberOf key="att.editLike"/>
    <memberOf key="att.typed"/>
    <memberOf key="att.naming"/>
    <memberOf key="model.persEventLike"/>
  </classes>
  <content>
<rng:choice>
<rng:ref name="event"/>
    <rng:group>
      <rng:optional>
        <rng:ref name="label"/>
      </rng:optional>
      <rng:optional>
        <rng:ref name="model.dateLike"/>
      </rng:optional>
      <rng:optional>
        <rng:ref name="placeName"/>
      </rng:optional>
      <rng:zeroOrMore>
        <rng:ref name="model.pLike"/>
      </rng:zeroOrMore>
      <rng:zeroOrMore>
        <rng:choice>
          <rng:ref name="model.noteLike"/>
          <rng:ref name="model.biblLike"/>
        </rng:choice>
      </rng:zeroOrMore>
    </rng:group>
</rng:choice>  </content>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples">
      <person>
        <event type="mat">
          <label>matriculation</label>
          <date value="1972-10-12">12 Oct 1972</date>
        </event>
        <event type="grad">
          <label>graduation</label>
          <date>23 June 1975</date>
        </event>
      </person>
    </egXML>
  </exemplum>
</elementSpec>
