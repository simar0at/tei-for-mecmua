<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="dictionaries" xml:id="DEF" usage="opt" ident="def">
  <equiv/>
  <gloss>definition</gloss>
  <gloss version="2007-05-04" xml:lang="es">definición</gloss>
  <gloss version="2006-10-28" xml:lang="ja">定義</gloss>
  <desc>contains definition text in a dictionary entry.</desc>
  <desc version="2007-05-04" xml:lang="es">contiene el texto de definición en una entrada de diccionario.</desc>
  <desc version="2006-10-28" xml:lang="ja"> 辞書項目における定義を示す．</desc>
  <classes>
    <memberOf key="model.entryParts.top"/>
    <memberOf key="model.entryParts"/>
    <memberOf key="att.lexicographic"/>
  </classes>
  <content>
    <rng:ref xmlns:rng="http://relaxng.org/ns/structure/1.0" name="macro.paraContent"/>
  </content>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples">
      <entry>
        <form>
          <orth>competitor</orth>
          <hyph>com|peti|tor</hyph>
          <pron>k@m"petit@(r)</pron>
        </form>
        <gramGrp>
          <pos>n</pos>
        </gramGrp>
        <def>person who competes.</def>
      </entry>
    </egXML>
  </exemplum>
  <remarks>
    <p/>
  </remarks>
  <listRef>
    <ptr target="#DITPDE"/>
  </listRef>
</elementSpec>
