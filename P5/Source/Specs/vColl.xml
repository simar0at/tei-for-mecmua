  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples">
      <fs>
        <f name="lex">
          <symbol value="auxquels"/>
        </f>
        <f name="maf">
          <vColl org="list">
            <fs>
              <f name="cat">
                <symbol value="prep"/>
              </f>
            </fs>
            <fs>
              <f name="cat">
                <symbol value="pronoun"/>
              </f>
              <f name="kind">
                <symbol value="rel"/>
              </f>
              <f name="num">
                <symbol value="pl"/>
              </f>
              <f name="gender">
                <symbol value="masc"/>
              </f>
            </fs>
          </vColl>
        </f>
      </fs>
    </egXML>
  </exemplum>
</elementSpec>
