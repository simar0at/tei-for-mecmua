<elementSpec xmlns="http://www.tei-c.org/ns/1.0"
	     xmlns:rng="http://relaxng.org/ns/structure/1.0"
module="namesdates" ident="listOrg">
  <equiv/>
  <desc>contains a list of descriptions, each of which provides
information about an identifiable organization. </desc>
  <classes>
    <memberOf key="att.typed"/>
    <memberOf key="att.declarable"/>
    <memberOf key="model.biblLike"/>
    <memberOf key="model.personLike"/>
  </classes>
  <content>
    <rng:choice>
      <rng:oneOrMore>
        <rng:ref name="model.pLike"/>
      </rng:oneOrMore>
      <rng:group>
        <rng:oneOrMore>
            <rng:ref name="org"/>
        </rng:oneOrMore>
      </rng:group>
    </rng:choice>
  </content>
  <remarks>
    <p rend="dataDesc">The type attribute may be used to distinguish lists
    of people of a particular type if convenient.</p>
  </remarks>
  <listRef>
    <ptr target="#NDORG"/>
  </listRef>
</elementSpec>
