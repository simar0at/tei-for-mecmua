<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="gaiji" xml:id="CHARDESC" ident="charDesc">
<equiv/>
<gloss>character description</gloss>
<desc>provides descriptive information about characters or
glyphs. </desc><desc version="2006-10-15" xml:lang="es">proporciona información descriptiva sobre carácteres o glifos.</desc>
<classes>
 <memberOf key="model.encodingPart"/>
</classes>
<content>
<!--(desc?, (char|glyph)+)-->
<rng:group xmlns:rng="http://relaxng.org/ns/structure/1.0">
      <rng:optional>
        <rng:ref name="desc"/>
      </rng:optional>
      <rng:oneOrMore>
      <rng:choice>
        <rng:ref name="char"/>
        <rng:ref name="glyph"/>
      </rng:choice>
      </rng:oneOrMore>
</rng:group>
</content>
  
<exemplum>
 <egXML xmlns="http://www.tei-c.org/ns/Examples"/>
</exemplum>
</elementSpec>
