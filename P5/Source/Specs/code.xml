<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="tagdocs" xml:id="CODE" usage="mwa" ident="code">
<equiv/>
<gloss/>
<desc>contains literal code from some formal language such as a
programming language</desc><desc version="2006-10-28" xml:lang="ja">プログラミング言語のような形式言語のコードを示す．</desc><desc version="2006-10-15" xml:lang="es">contiene código literal.</desc>
  <classes>
    <memberOf key="model.emphLike"/>
  </classes>
  <content>
    <rng:text xmlns:rng="http://relaxng.org/ns/structure/1.0"/>
  </content>
  <attList>
    <attDef ident="lang" usage="opt">
	<equiv/>
	<desc>a name identifying the formal language in which  the
	code is expressed</desc><desc version="2006-10-28" xml:lang="ja">当該コードの形式言語名を示す．</desc>
      <datatype><rng:ref xmlns:rng="http://relaxng.org/ns/structure/1.0" name="data.word"/></datatype>
    </attDef>
  </attList>
</elementSpec>
