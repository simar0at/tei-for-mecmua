<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="dictionaries" xml:id="USG" usage="opt" ident="usg">
  <equiv/>
  <gloss>usage</gloss>
  <gloss version="2007-05-04" xml:lang="es">uso</gloss>
  <gloss version="2006-10-28" xml:lang="ja">用例</gloss>
  <desc>contains usage information in a dictionary entry.</desc>
  <desc version="2007-05-04" xml:lang="es">contiene la información sobre el uso en la entrada de un diccionario. </desc>
  <desc version="2006-10-28" xml:lang="ja"> 辞書項目の用例に関する情報を示す．</desc>
  <classes>
    <memberOf key="model.entryParts.top"/>
    <memberOf key="model.entryParts"/>
    <memberOf key="model.gramPart"/>
    <memberOf key="att.lexicographic"/>
  </classes>
  <content>
    <rng:ref xmlns:rng="http://relaxng.org/ns/structure/1.0" name="macro.paraContent"/>
  </content>
  <attList>
    <attDef ident="type" usage="opt">
      <equiv/>
      <desc>classifies the usage information using any convenient typology.</desc>
      <desc version="2007-05-04" xml:lang="es">clasifica la información sobre el uso aplicando una tipología funcional.</desc>
      <desc version="2006-10-28" xml:lang="ja">当該用例情報の分類を示す．</desc>
      <datatype>
        <rng:ref xmlns:rng="http://relaxng.org/ns/structure/1.0" name="data.enumerated"/>
      </datatype>
      <valList type="open">
        <valItem ident="geo">
          <equiv/>
          <gloss>geographic area</gloss>
          <gloss version="2007-05-04" xml:lang="es">área geográfica</gloss>
        </valItem>
        <valItem ident="time">
          <equiv/>
          <gloss>temporal, historical era (archaic, old, etc.)</gloss>
          <gloss version="2007-05-04" xml:lang="es">temporal, período histórico (arcaico, antiguo, etc.)</gloss>
        </valItem>
        <valItem ident="dom">
          <equiv/>
          <gloss>domain</gloss>
          <gloss version="2007-05-04" xml:lang="es">dominio</gloss>
        </valItem>
        <valItem ident="reg">
          <equiv/>
          <gloss>register</gloss>
          <gloss version="2007-05-04" xml:lang="es">registro</gloss>
        </valItem>
        <valItem ident="style">
          <equiv/>
          <gloss>style (figurative, literal, etc.)</gloss>
          <gloss version="2007-05-04" xml:lang="es">estilo (figurativo, literal, etc.)</gloss>
        </valItem>
        <valItem ident="plev">
          <equiv/>
          <gloss>preference level (chiefly, usually, etc.)</gloss>
          <gloss version="2007-05-04" xml:lang="es">nivel de preferencia (principal, habitual)</gloss>
        </valItem>
        <valItem ident="lang">
          <equiv/>
          <gloss>lang (language for foreign words, spellings pronunciations, etc.)</gloss>
          <gloss version="2007-05-04" xml:lang="es">lang (lengua para palabras extranjeras, pronunciación de la ortografía, etc.)</gloss>
        </valItem>
        <valItem ident="gram">
          <equiv/>
          <gloss>grammatical usage</gloss>
          <gloss version="2007-05-04" xml:lang="es">uso gramatical</gloss>
        </valItem>
        <valItem ident="syn">
          <equiv/>
          <gloss>synonym given to show use</gloss>
          <gloss version="2007-05-04" xml:lang="es">sinónimo dado para ilustrar el uso</gloss>
        </valItem>
        <valItem ident="hyper">
          <equiv/>
          <gloss>hypernym given to show usage</gloss>
          <gloss version="2007-05-04" xml:lang="es">hiperónimo dado para ilustrar el uso</gloss>
        </valItem>
        <valItem ident="colloc">
          <equiv/>
          <gloss>collocation given to show usage</gloss>
          <gloss version="2007-05-04" xml:lang="es">colocación dada para ilustrar el uso</gloss>
        </valItem>
        <valItem ident="comp">
          <equiv/>
          <gloss>typical complement</gloss>
          <gloss version="2007-05-04" xml:lang="es">complemento habitual</gloss>
        </valItem>
        <valItem ident="obj">
          <equiv/>
          <gloss>typical object</gloss>
          <gloss version="2007-05-04" xml:lang="es">objeto habitual</gloss>
        </valItem>
        <valItem ident="subj">
          <equiv/>
          <gloss>typical subject</gloss>
          <gloss version="2007-05-04" xml:lang="es">sujeto habitual</gloss>
        </valItem>
        <valItem ident="verb">
          <equiv/>
          <gloss>typical verb</gloss>
          <gloss version="2007-05-04" xml:lang="es">verbo habitual</gloss>
        </valItem>
        <valItem ident="hint">
          <equiv/>
          <gloss>unclassifiable piece of information to guide sense choice</gloss>
          <gloss version="2007-05-04" xml:lang="es">información no clasificable para mostrar información del significado.</gloss>
        </valItem>
      </valList>
      <exemplum>
        <egXML xmlns="http://www.tei-c.org/ns/Examples"/>
      </exemplum>
      <remarks>
        <p/>
      </remarks>
    </attDef>
  </attList>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples">
      <form>
        <orth>colour</orth>
        <form>
          <usg type="geo">U.S.</usg>
          <orth>color</orth>
        </form>
      </form>
    </egXML>
  </exemplum>
  <remarks>
    <p/>
  </remarks>
  <listRef>
    <ptr target="#DITPUS"/>
  </listRef>
</elementSpec>
