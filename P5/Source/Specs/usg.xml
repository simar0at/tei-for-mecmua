<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="dictionaries" xml:id="USG" usage="opt" ident="usg">
  <equiv/>
  <gloss>usage</gloss>
  <gloss version="2007-01-21" xml:lang="it">uso</gloss>
  <gloss version="2007-05-04" xml:lang="es">uso</gloss>
  <gloss version="2006-10-28" xml:lang="ja">用例</gloss>
  <desc>contains usage information in a dictionary entry.</desc>
  <desc version="2007-01-21" xml:lang="it">in una voce di dizionario, contiene informazioni sull'uso.</desc>
  <desc version="2007-05-04" xml:lang="es">contiene la información sobre el uso en la entrada de un diccionario. </desc>
  <desc version="2006-10-28" xml:lang="ja"> 辞書項目の用例に関する情報を示す．</desc>
  <classes>
    <memberOf key="model.entryParts.top"/>
    <memberOf key="model.entryParts"/>
    <memberOf key="model.gramPart"/>
    <memberOf key="att.lexicographic"/>
  </classes>
  <content>
    <rng:ref xmlns:rng="http://relaxng.org/ns/structure/1.0" name="macro.paraContent"/>
  </content>
  <attList>
    <attDef ident="type" usage="opt">
      <equiv/>
      <desc>classifies the usage information using any convenient typology.</desc>
      <desc version="2007-01-21" xml:lang="it">classifica le informazioni sull'uso secondo una  tipologia funzionale.</desc>
      <desc version="2007-05-04" xml:lang="es">clasifica la información sobre el uso aplicando una tipología funcional.</desc>
      <desc version="2006-10-28" xml:lang="ja">当該用例情報の分類を示す．</desc>
      <datatype>
        <rng:ref xmlns:rng="http://relaxng.org/ns/structure/1.0" name="data.enumerated"/>
      </datatype>
      <valList type="open">
        <valItem ident="geo">
          <gloss>geographic</gloss>
          <gloss version="2007-08-25" xml:lang="it">geographic area</gloss>
          <gloss version="2007-01-21" xml:lang="it">area geografica</gloss>
          <desc>geographic area</desc>
          <desc version="2007-05-04" xml:lang="es">área geográfica</desc>
        </valItem>
        <valItem ident="time">
          <equiv/>
          <desc>temporal, historical era (archaic, old, etc.)</desc>
          <desc version="2007-05-04" xml:lang="es">temporal, período histórico (arcaico, antiguo, etc.)</desc>
        </valItem>
        <valItem ident="dom">
          <equiv/>
          <gloss>domain</gloss>
          <gloss version="2007-01-21" xml:lang="it">ambito</gloss>
          <gloss version="2007-05-04" xml:lang="es">dominio</gloss>
        </valItem>
        <valItem ident="reg">
          <equiv/>
          <gloss>register</gloss>
          <gloss version="2007-01-21" xml:lang="it">registro</gloss>
          <gloss version="2007-05-04" xml:lang="es">registro</gloss>
        </valItem>
        <valItem ident="style">
          <equiv/>
          <desc>style (figurative, literal, etc.)</desc>
          <desc version="2007-05-04" xml:lang="es">estilo (figurativo, literal, etc.)</desc>
        </valItem>
        <valItem ident="plev">
          <gloss>preference level</gloss>
          <gloss version="2007-08-25" xml:lang="it">preference level (chiefly, usually, etc.)</gloss>
          <gloss version="2007-01-21" xml:lang="it">livello di preferenza (principalmente, di solito, ecc.)</gloss>
          <desc>preference level (chiefly, usually, etc.)</desc>
          <desc version="2007-05-04" xml:lang="es">nivel de preferencia (principal, habitual)</desc>
        </valItem>
        <valItem ident="lang">
          <gloss>language</gloss>
          <gloss version="2007-08-25" xml:lang="it">lang (language for foreign words, spellings pronunciations, etc.)</gloss>
          <gloss version="2007-01-21" xml:lang="it">lang (lingua per parole straniere, ortografia, pronuncie, ecc.)</gloss>
          <desc>lang (language for foreign words, spellings pronunciations, etc.)</desc>
          <desc version="2007-08-25" xml:lang="it">name of a language mentioned in etymological or other linguistic discussion.</desc>
          <desc version="2007-01-21" xml:lang="it">il nome di una lingua menzionata nell'etimologia o in altra discussione linguistica.</desc>
          <desc version="2007-05-04" xml:lang="es">lang (lengua para palabras extranjeras, pronunciación de la ortografía, etc.)</desc>
        </valItem>
        <valItem ident="gram">
          <gloss>grammatical</gloss>
          <gloss version="2007-08-25" xml:lang="it">grammatical usage</gloss>
          <gloss version="2007-01-21" xml:lang="it">uso grammaticale</gloss>
          <desc>grammatical usage</desc>
          <desc version="2007-08-25" xml:lang="it">within an entry in a dictionary or a terminological data file, contains grammatical
            information relating to a term, word, or form.</desc>
          <desc version="2007-01-21" xml:lang="it">all'interno di una voce in un dizionario o di un file terminologico, contiene informazioni relative a un termine, una parola o una forma.</desc>
          <desc version="2007-05-04" xml:lang="es">uso gramatical</desc>
        </valItem>
        <valItem ident="syn">
          <gloss>synonym</gloss>
          <gloss version="2007-08-25" xml:lang="it">synonym given to show use</gloss>
          <gloss version="2007-01-21" xml:lang="it">sinonimo fonito per mostrane l'uso</gloss>
          <desc>synonym given to show use</desc>
          <desc version="2007-05-04" xml:lang="es">sinónimo dado para ilustrar el uso</desc>
        </valItem>
        <valItem ident="hyper">
          <gloss>hypernym</gloss>
          <gloss version="2007-08-25" xml:lang="it">hypernym given to show usage</gloss>
          <gloss version="2007-01-21" xml:lang="it">iperonimo fonito per mostrane l'uso</gloss>
          <desc>hypernym given to show usage</desc>
          <desc version="2007-05-04" xml:lang="es">hiperónimo dado para ilustrar el uso</desc>
        </valItem>
        <valItem ident="colloc">
          <gloss>collocation</gloss>
          <gloss version="2007-08-25" xml:lang="it">collocation given to show usage</gloss>
          <gloss version="2007-01-21" xml:lang="it">collocazione fonita per mostrane l'uso</gloss>
          <desc>collocation given to show usage</desc>
          <desc version="2007-08-25" xml:lang="it">contains a collocate of the headword.</desc>
          <desc version="2007-01-21" xml:lang="it">contiene il collocato di un lemma.</desc>
          <desc version="2007-05-04" xml:lang="es">colocación dada para ilustrar el uso</desc>
        </valItem>
        <valItem ident="comp">
          <gloss>complement</gloss>
          <gloss version="2007-08-25" xml:lang="it">typical complement</gloss>
          <gloss version="2007-01-21" xml:lang="it">complemento tipico</gloss>
          <desc>typical complement</desc>
          <desc version="2007-05-04" xml:lang="es">complemento habitual</desc>
        </valItem>
        <valItem ident="obj">
          <gloss>object</gloss>
          <gloss version="2007-08-25" xml:lang="it">typical object</gloss>
          <gloss version="2007-01-21" xml:lang="it">oggetto tipico</gloss>
          <desc>typical object</desc>
          <desc version="2007-05-04" xml:lang="es">objeto habitual</desc>
        </valItem>
        <valItem ident="subj">
          <gloss>subject</gloss>
          <gloss version="2007-08-25" xml:lang="it">typical subject</gloss>
          <gloss version="2007-01-21" xml:lang="it">soggetto tipico</gloss>
          <desc>typical subject</desc>
          <desc version="2007-05-04" xml:lang="es">sujeto habitual</desc>
        </valItem>
        <valItem ident="verb">
          <equiv/>
          <desc>typical verb</desc>
          <desc version="2007-05-04" xml:lang="es">verbo habitual</desc>
        </valItem>
        <valItem ident="hint">
          <equiv/>
          <desc>unclassifiable piece of information to guide sense choice</desc>
          <desc version="2007-05-04" xml:lang="es">información no clasificable para mostrar información del significado.</desc>
        </valItem>
      </valList>
      <exemplum>
        <egXML xmlns="http://www.tei-c.org/ns/Examples"/>
      </exemplum>
      <remarks>
        <p/>
      </remarks>
    </attDef>
  </attList>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples">
      <form>
        <orth>colour</orth>
        <form>
          <usg type="geo">U.S.</usg>
          <orth>color</orth>
        </form>
      </form>
    </egXML>
  </exemplum>
  <remarks>
    <p/>
  </remarks>
  <listRef>
    <ptr target="#DITPUS"/>
  </listRef>
</elementSpec>
