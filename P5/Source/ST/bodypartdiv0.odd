<macroSpec xmlns="http://www.tei-c.org/ns/1.0" module="tei" type="pe" ident="macro.bodyPart.div0">
  <gloss>a set of common elements in <gi>body</gi></gloss>
  <desc>groups together a sequence of declarations which are used in
  the definition of <ident type="class">macro.bodyPart.main</ident>,
  which is itself used in the defintion of the <gi>body</gi>
  element</desc>
  <content xmlns:rng="http://relaxng.org/ns/structure/1.0">
    <rng:group>      <!-- this <group> exists just so odd2dtd can put a comma in right place  -->
      <rng:ref name="div0"/>
      <rng:zeroOrMore>
	<rng:choice>
	  <rng:ref name="div0"/>
	  <rng:ref name="divGen"/>
	  <rng:group>      <!-- this <group> exists just so odd2dtd can get parens in right place  -->
	    <rng:ref name="model.global"/>
	  </rng:group>
	</rng:choice>
      </rng:zeroOrMore>
    </rng:group>
  </content>
  <listRef>
    <ptr target="#DS"/>
    <ptr target="#STECMI"/>
  </listRef>
</macroSpec>
