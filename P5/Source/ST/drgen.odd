<macroSpec xmlns="http://www.tei-c.org/ns/1.0" depend="drama" module="core" xml:id="DRGEN" type="pe" ident="gen.drama">
<gloss>drama part of general-base component sequence</gloss>
<desc>contains a string used in constructing the definition of
component sequence used in the general base tag set.</desc>
  <content>
    <rng:group xmlns:rng="http://relaxng.org/ns/structure/1.0">
<?teidtd (?>
      <rng:ref name="tei.comp.drama"/>
<?teidtd )?>
      <rng:zeroOrMore>
        <rng:choice>
          <rng:ref name="tei.common"/>
          <rng:ref name="tei.comp.drama"/>
        </rng:choice>
      </rng:zeroOrMore>
    </rng:group>
<?teidtd |?>
  </content>
  <listRef>
    <ptr target="#STEC"/>
  </listRef>
</macroSpec>
