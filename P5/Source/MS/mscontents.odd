<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="msdescription" xml:id="MSCONTENTS" ident="msContents">
<equiv/>
<gloss/>
<desc xml:id="mscontents.desc">describes the intellectual content of a
manuscript or manuscript part, either as a series of paragraphs or as a
series of structured manuscript items.</desc>
  <classes/>
  <content>
<!--<!ELEMENT msContents  - -  (p+|(overview?, msItem+))>-->
<rng:choice xmlns:rng="http://relaxng.org/ns/structure/1.0">
    <rng:oneOrMore>
      <rng:ref name="model.pLike"/>
    </rng:oneOrMore>
<rng:group>
    <rng:optional>
      <rng:ref name="summary"/>
    </rng:optional>
<rng:oneOrMore>
<rng:choice>
<rng:ref name="msItem"/>
<rng:ref name="msItemStruct"/>
</rng:choice>
</rng:oneOrMore>
</rng:group>
</rng:choice>
  </content>
  <attList>
    <attDef ident="class">
	<equiv/>
	<gloss/>
	<desc>identifies the text types or classifications applicable
	to this object</desc>
      <datatype><rng:ref xmlns:rng="http://relaxng.org/ns/structure/1.0" name="data.code"/></datatype>
      <valDesc>One or more codes, each of which is used as the identifier for
a text classification element supplied in the TEI Header <gi>textClass</gi>
element.</valDesc>
<!-- better: make it a member of declaring -->
    </attDef>
    <attDef ident="defective">
	<equiv/>
	<gloss/>
	<desc>indicates whether the object described is defective, i.e. incomplete.</desc>
      <datatype><rng:ref xmlns:rng="http://relaxng.org/ns/structure/1.0" name="data.xTruthValue"/></datatype>
      <valDesc>true, false, unknown, or unspecified</valDesc>
    </attDef>
  </attList>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples" xmlns:rng="http://relaxng.org/ns/structure/1.0"><msContents>
<p>A collection of Lollard sermons</p>
</msContents></egXML>
</exemplum>
<exemplum><egXML xmlns="http://www.tei-c.org/ns/Examples" xmlns:rng="http://relaxng.org/ns/structure/1.0"><msContents>
<msItem n="1"><locus>fols. 5r-7v</locus>
<title>An ABC</title> 
<bibl><title>IMEV</title> <biblScope>239</biblScope></bibl></msItem>
<msItem n="2"><locus>fols. 7v-8v</locus>
<title xml:lang="FR">Lenvoy de Chaucer a Scogan</title>
<bibl><title>IMEV</title>
<biblScope>3747</biblScope></bibl></msItem>
<msItem n="3"><locus>fol. 8v</locus><title>Truth</title> 
<bibl><title>IMEV</title> <biblScope>809</biblScope></bibl></msItem>
<msItem n="4"><locus>fols. 8v-10v</locus>
<title>Birds Praise of Love</title>
<bibl><title>IMEV</title> <biblScope>1506</biblScope></bibl></msItem>
<msItem n="5"><locus>fols. 10v-11v</locus>
<title xml:lang="LA">De amico ad amicam</title>
<title xml:lang="LA">Responcio</title> 
<bibl><title>IMEV</title> <biblScope>16 &amp; 19</biblScope></bibl></msItem>
<msItem n="6"><locus>fols. 14r-126v</locus>
<title>Troilus and Criseyde</title> 
<note>Bk. 1:71-Bk. 5:1701, with additional losses due to
mutilation throughout</note>
</msItem>
</msContents></egXML>
  </exemplum>
  <listRef>
    <ptr target="#msco"/>
  </listRef>
</elementSpec>
