<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="msdescription" xml:id="ADDITIONAL" ident="additional">
<equiv/>
<gloss/>
<desc xml:id="additional.desc">groups additional information, comprising
bibliographic information about a manuscript, its current curatorial status, and
any other associated materials. </desc>
  <classes/>
  <content>
<!--<!ELEMENT additional  - -  (adminInfo?, surrogates?, listBibl?)>
-->
<rng:group xmlns:rng="http://relaxng.org/ns/structure/1.0">
    <rng:optional>
      <rng:ref name="adminInfo"/>
    </rng:optional>
    <rng:optional>
      <rng:ref name="surrogates"/>
    </rng:optional>
    <rng:optional>
      <rng:ref name="listBibl"/>
    </rng:optional>
</rng:group>
  </content>
  <attList/>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples"><additional>
<adminInfo>
  <recordHist>
   <!-- record history here -->
  </recordHist>
<custodialHist>
  <!-- custodial history here -->
</custodialHist> 
</adminInfo>
<surrogates>
  <!-- information about surrogates here -->
</surrogates>
<listBibl>
  <!-- full bibliography here -->
</listBibl>
</additional></egXML>
  </exemplum>
  <listRef>
    <ptr target="#msad"/>
  </listRef>
</elementSpec>
