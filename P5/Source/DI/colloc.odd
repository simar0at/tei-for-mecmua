<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="dictionaries" xml:id="COLLOC" usage="opt" ident="colloc">
<equiv/>
<gloss>collocate</gloss>
<desc>contains a collocate of the headword.</desc>
    <classes>
        <memberOf key="model.entryParts"/>
        <memberOf key="model.gramPart"/>
        <memberOf key="att.lexicographic"/>
    </classes>
    <content>
        <rng:ref xmlns:rng="http://relaxng.org/ns/structure/1.0" name="macro.paraContent"/>
    </content>
    <attList>
        <attDef ident="type" usage="opt">
	<equiv/>
	<desc>classifies the collocation, using any convenient typology.</desc>
            <datatype>
	      <rng:ref xmlns:rng="http://relaxng.org/ns/structure/1.0" name="data.name"/>
            </datatype>
            <valDesc>any string of XML name characters, e.g. <mentioned>preposition</mentioned>.</valDesc>
            <exemplum>
                <egXML xmlns="http://www.tei-c.org/ns/Examples"/>
            </exemplum>
            <remarks>
                <p/>
            </remarks>
        </attDef>
    </attList>
    <exemplum>
        <egXML xmlns="http://www.tei-c.org/ns/Examples"/>
    </exemplum>
    <remarks>
        <p/>
    </remarks>
    <listRef>
        <ptr target="#DITPGR"/>
    </listRef>
</elementSpec>
