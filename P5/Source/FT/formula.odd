<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="figures" xml:id="FORMULA" usage="rwa" ident="formula">
<equiv/>
<gloss/>
<desc>contains a mathematical or other formula.</desc>
  <classes>
    <memberOf key="model.phrase"/>
  </classes>
  <content>
    <rng:text xmlns:rng="http://relaxng.org/ns/structure/1.0"/>
  </content>
  <attList>
    <attDef ident="notation" usage="opt">
	<equiv/>
	<desc>supplies the name of a previously defined notation used for the
content of the
element.</desc>
      <datatype><rng:ref xmlns:rng="http://relaxng.org/ns/structure/1.0" name="data.code"/></datatype>
      <valDesc>The
name of a formal notation previously declared in the document type declaration.</valDesc>
      <remarks/>
    </attDef>
  </attList>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples"/>
  </exemplum>
  <remarks>
    <p rend="dataDesc">The content model for this element is specified by the
parameter entity <term>formulaContent</term>, the default value of
which is <code>#PCDATA</code>.</p>
    <p/>
  </remarks>
  <listRef>
    <ptr target="#FTFOR" type="div1"/>
  </listRef>
</elementSpec>
