<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="core" xml:id="EMPH" usage="rwa" ident="emph">
<equiv/>
<gloss>emphasized</gloss>
<desc>marks words or phrases which are stressed or emphasized for
linguistic or rhetorical effect.</desc>
  <classes>
    <memberOf key="tei.hqphrase"/>
  </classes>
  <content>
    <rng:ref xmlns:rng="http://relaxng.org/ns/structure/1.0" name="macro.paraContent"/>
  </content>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples">You took the car and did <emph>what</emph>?!!</egXML>
  </exemplum>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples"><q>What it all comes to is this,</q><![CDATA[ he said. ]]><q><emph>What
does Christopher Robin do in the morning nowadays?</emph></q></egXML>
  </exemplum>
  <remarks/>
  <listRef>
    <ptr target="#COHQHE"/>
    <ptr target="#COHQH"/>
  </listRef>
</elementSpec>
