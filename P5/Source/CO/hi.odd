<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="core" xml:id="HI" usage="opt" ident="hi">
<equiv/>
<gloss>highlighted</gloss>
<desc>marks a word or phrase as graphically distinct from the
surrounding text, for reasons concerning which no claim is
made. </desc>
  <classes>
    <memberOf key="tei.hqphrase"/>
  </classes>
  <content>
    <rng:ref xmlns:rng="http://relaxng.org/ns/structure/1.0" name="macro.paraContent"/>
  </content>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples"><hi rend="gothic">And this Indenture further witnesseth</hi><![CDATA[
that the said ]]><hi rend="italic">Walter Shandy</hi><![CDATA[, merchant,
in consideration of the said intended marriage ...]]></egXML>
  </exemplum>
  <remarks/>
  <listRef>
    <ptr target="#COHQHE"/>
    <ptr target="#COHQH"/>
  </listRef>
</elementSpec>
