<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="core" xml:id="PUBLISHR" usage="opt" ident="publisher">
<equiv/>
<gloss/>
<desc>provides the name of the organization responsible for
the publication or distribution of a bibliographic item.</desc>
  <classes>
    <memberOf key="model.biblPart"/>
    <memberOf key="model.imprintPart"/>
  </classes>
  <content>
    <rng:ref xmlns:rng="http://relaxng.org/ns/structure/1.0" name="macro.phraseSeq"/>
  </content>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples">      <imprint>
        <pubPlace>Oxford</pubPlace>
        <publisher>Clarendon Press</publisher>
        <date>1987</date>
      </imprint>
    </egXML>
  </exemplum>
  <remarks>
    <p rend="dataDesc">Use the full form  of the name by which a company is usually
referred to, rather than any abbreviation of it which may appear on a
title page</p>
  </remarks>
  <listRef>
    <ptr target="#COBICOI"/>
    <ptr target="#HD24"/>
  </listRef>
</elementSpec>
