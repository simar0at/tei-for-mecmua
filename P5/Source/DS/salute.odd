<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="textstructure" xml:id="SALUTE" usage="opt" ident="salute">
<equiv/>
<gloss>salutation</gloss>
<desc>contains a salutation or greeting prefixed to a foreword,
dedicatory epistle, or other division of a text, or the
salutation in the closing of a letter, preface, etc.</desc>
  <classes>
    <memberOf key="model.divWrapper"/>
    <memberOf key="model.divWrapper.bottom"/>
  </classes>
  <content>
    <rng:ref xmlns:rng="http://relaxng.org/ns/structure/1.0" name="macro.phraseSeq"/>
  </content>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples">      <salute>To all courteous mindes, that will voutchsafe the
readinge.</salute>
    </egXML>
  </exemplum>
  <remarks/>
  <listRef>
    <ptr target="#DSCO"/>
    <ptr target="#DSOC"/>
  </listRef>
</elementSpec>
