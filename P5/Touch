#!/bin/sh
cd `dirname $0`
cd ..
touch /tmp/changed
/usr/bin/svn -q update P5 Stylesheets MM2007 jerusalem
find P5 Stylesheets -newer /tmp/changed | grep -v .svn > /tmp/changed.files
if test -s /tmp/changed.files 
then 
 cd P5
 make OTHERLANGUAGES= html-web
 make OTHERLANGUAGES= html-web-beta
fi
rm /tmp/changed.files /tmp/changed
