<moduleSpec xmlns="http://www.tei-c.org/ns/1.0" ident="textstructure">
  <elementSpec xmlns:s="http://www.ascc.net/xml/schematron" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ident="TEI" module="textstructure">
  
  <gloss>TEI document</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>contains a single TEI-conformant document,
comprising a TEI header and a text, either in isolation or as part of a
<gi>teiCorpus</gi> element.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  <attList>
    <attDef ident="version" usage="opt">
      
      <desc>The version of the TEI scheme</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
      
      
      
      
      
      
      
      
      <valDesc>A number identifying the version of the TEI guidelines</valDesc><valDesc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</valDesc>
    </attDef>
  </attList>
  
  
  
</elementSpec>
  <elementSpec ident="text" module="textstructure">
  
  <gloss/><gloss xml:lang="pl" notBefore="2007-12-21"/>
  
  
  <desc>contains a single text of any kind, whether unitary or
composite, for example a poem or drama, a collection of essays, a novel,
a dictionary, or a corpus sample.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="body" module="textstructure">
  
  <gloss>text body</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>contains the whole body of a single unitary text, excluding any front or back matter.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="group" module="textstructure">
  
  <gloss/><gloss xml:lang="pl" notBefore="2007-12-21"/>
  
  
  <desc>contains the body of a composite text, grouping together a
sequence of distinct texts (or groups of such texts) which are regarded
as a unit for some purpose, for example the collected works of an
author, a sequence of prose essays, etc.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="floatingText" module="textstructure">
  
  <gloss/><gloss xml:lang="pl" notBefore="2007-12-21"/>
  
  <desc>contains a single text of any kind, whether unitary or
composite, which interrupts the
text containing it at any point and after which the surrounding text
resumes.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="div" module="textstructure">
  
  <gloss>text division</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>contains a subdivision of the front, body, or back of a
text.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="div1" module="textstructure">
  
  <gloss>level-1 text division</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>contains a first-level subdivision of the front, body, or back
of a text.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="div2" module="textstructure">
  
  <gloss>level-2 text division</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>contains a second-level subdivision of the front, body, or back of a
 text.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="div3" module="textstructure">
  
  <gloss>level-3 text division</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>contains a third-level subdivision of the front, body, or back of a
 text.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="div4" module="textstructure">
  
  <gloss>level-4 text division</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>contains a fourth-level subdivision of the front, body, or back of a
 text.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="div5" module="textstructure">
  
  <gloss>level-5 text division</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>contains a fifth-level subdivision of the front, body, or back of a
 text.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="div6" module="textstructure">
  
  <gloss>level-6 text division</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>contains a sixth-level subdivision of the front, body, or back of a
 text.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="div7" module="textstructure">
  
  <gloss>level-7 text division</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>contains the smallest possible subdivision of the front, body or
back of a text, larger than a paragraph.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="trailer" module="textstructure">
  
  <desc>contains a closing title or footer appearing at the end of
a division of a text. </desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="byline" module="textstructure">
  
  <gloss/><gloss xml:lang="pl" notBefore="2007-12-21"/>
  
  
  <desc>contains the primary statement of responsibility given for a work
on its title page or at the head or end of the work.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="dateline" module="textstructure">
  
  <gloss/><gloss xml:lang="pl" notBefore="2007-12-21"/>
  
  
  <desc>contains a brief description of the place, date, time, etc. of
	production of a letter, newspaper story, or other work, prefixed or
	suffixed to it as a kind of heading or trailer. </desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="argument" module="textstructure">
  
  <gloss/><gloss xml:lang="pl" notBefore="2007-12-21"/>
  
  
  <desc>A formal list or prose description of the topics addressed by
	a subdivision of a text.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="epigraph" module="textstructure">
  
  <desc>contains a quotation, anonymous or attributed, appearing at
the start of a section or chapter, or on a title page. </desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="opener" module="textstructure">
  
  <gloss/><gloss xml:lang="pl" notBefore="2007-12-21"/>
  
  
  <desc>groups together dateline, byline, salutation, and similar
phrases appearing as a preliminary group at the start of a
division, especially of a letter.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="closer" module="textstructure">
  
  <gloss/><gloss xml:lang="pl" notBefore="2007-12-21"/>
  
  
  <desc>groups together salutations, datelines, and similar
phrases appearing as a final group at the end of a
division, especially of a letter.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="salute" module="textstructure">
  
  <gloss>salutation</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>contains a salutation or greeting prefixed to a foreword,
dedicatory epistle, or other division of a text, or the
salutation in the closing of a letter, preface, etc.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="signed" module="textstructure">
  
  <gloss>signature</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
  
  
  
  
  
  <desc>contains the closing salutation, etc., appended to a foreword,
dedicatory epistle, or other division of a text. </desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="postscript" module="textstructure">
  <desc>contains a postscript, e.g. to a letter.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
</elementSpec>
  <elementSpec ident="titlePage" module="textstructure">
  
  <gloss>title page</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>contains the title page of a text, appearing within the front
or back matter. </desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  <attList>
    <attDef ident="type" usage="opt">
      
      <desc>classifies the title page according to any convenient typology.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
      
      
      
      
      
      
      
      <valDesc>Any string, e.g. <mentioned>full</mentioned>, <mentioned>half</mentioned>, <mentioned>Series</mentioned>, etc.</valDesc><valDesc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</valDesc>
      
    </attDef>
  </attList>
  
  
</elementSpec>
  <elementSpec ident="docTitle" module="textstructure">
  
  <gloss>document title</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>contains the title of a document, including all its
constituents, as given on a title page.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="titlePart" module="textstructure">
  
  <desc>contains a subsection or division of the title of a work, as
indicated on a title page.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  <attList>
    <attDef ident="type" usage="opt">
      
      <desc>specifies the role of this subdivision of the title.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
      
      
      
      
      
      
      
      
      <valList type="semi">
        <valItem ident="main">
          
          <desc>main title of the work
        </desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
          
          
        </valItem>
        <valItem ident="sub">
          <gloss>subordinate</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
          
          
          
          
          <desc>subtitle  of the work
        </desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
          
          
        </valItem>
        <valItem ident="alt">
          <gloss>alternate</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
          
          
          
          
          <desc>alternative title  of the work
        </desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
          
          
        </valItem>
        <valItem ident="short">
          
          <desc>abbreviated form of title</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
          
          
        </valItem>
        <valItem ident="desc">
          <gloss>descriptive</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
          
          
          
          
          <desc>descriptive paraphrase of the work 
   </desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
          
          
        </valItem>
      </valList>
    </attDef>
  </attList>
  
  
</elementSpec>
  <elementSpec ident="docAuthor" module="textstructure">
  
  <gloss>document author</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>contains the name of the author of the document, as given on the
title page (often but not always contained in a byline).</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="imprimatur" module="textstructure">
  
  <gloss/><gloss xml:lang="pl" notBefore="2007-12-21"/>
  
  <desc>contains a formal statement authorizing the publication of
a work, sometimes required to appear on a title page or its verso.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="docEdition" module="textstructure">
  
  <gloss>document edition</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>contains an edition statement as presented on a title page of a
document.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="docImprint" module="textstructure">
  
  <gloss>document imprint</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>contains the imprint statement (place and date of publication,
publisher name), as given
(usually) at the foot of a title page.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="docDate" module="textstructure">
  
  <gloss>document date</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>contains the date of a document, as given
(usually) on a title page.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  <attList>
    <attDef ident="when" usage="opt">
      
      <desc>gives the value of the date in standard form, i.e. YYYY-MM-DD.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
      
      
      
      <valDesc>a date in one of the formats specified in <title>XML
      Schema Part 2: Datatypes Second Edition</title></valDesc><valDesc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</valDesc>
      
    </attDef>
  </attList>
  
  
  
</elementSpec>
  <elementSpec ident="front" module="textstructure">
  
  <gloss>front matter</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>contains any prefatory matter (headers,
title page, prefaces, dedications, etc.)
found  at the start of a document, before the main body.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="back" module="textstructure">
  
  <gloss>back matter</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>contains any appendixes, etc. following the main part of a
    text.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
  
</elementSpec>
</moduleSpec>
