<moduleSpec xmlns="http://www.tei-c.org/ns/1.0" ident="dictionaries">
  <classSpec ident="model.entryLike" module="dictionaries">
  <desc>groups elements structurally analogous to paragraphs within 
 dictionaries.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
</classSpec>
  <classSpec ident="att.entryLike" module="dictionaries">
  <desc>groups the different styles of dictionary entries.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  <attList>
    <attDef ident="type" usage="rwa">
      
      <desc>indicates type of entry, in dictionaries with multiple types.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
      
      
      
      
      
      
      <valList type="semi">
        <valItem ident="main">
          
          <desc>a main entry (default).</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="hom">
          <gloss>homograph</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
          
          
          
          <desc version="2007-08-25">groups information relating to one homograph within an entry.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="xref">
          <gloss>cross reference</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
          
          
          
          <desc version="2007-08-25">a reduced entry whose only function is to point to another main entry
                            (e.g. for forms of an irregular verb or for variant spellings:  <mentioned>was</mentioned> pointing to <mentioned>be</mentioned>, or                                <mentioned>esthete</mentioned> to <mentioned>aesthete</mentioned>).</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
          
          <desc>a reduced entry whose only function is to point to another main entry
                        (e.g. for forms of an irregular verb or for variant spellings:
                        <mentioned>was</mentioned> pointing to <mentioned>be</mentioned>, or
                        <mentioned>esthete</mentioned> to <mentioned>aesthete</mentioned>).</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="affix">
          <desc>an entry for a prefix, infix, or suffix.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="abbr">
          <gloss>abbreviation</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
          
          
          
          <desc version="2007-08-25">an entry for an abbreviation.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="supplemental">
          <desc>a supplemental entry (for use in dictionaries which issue supplements to
                        their main work in which they include updated information about entries).</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="foreign">
          <desc>an entry for a foreign word in a monolingual dictionary.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
          
        </valItem>
      </valList>
    </attDef>
    <attDef ident="sortKey" usage="opt">
      <desc>contains a (sortable) character sequence reflecting the entry's alphabetical
                position in the printed dictionary.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
      
      
      
      
      
      <valDesc>any sequence of characters which, when sorted with the other values, will
                produced the desired order; specifics of sort key construction are application-dependent.</valDesc><valDesc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</valDesc>
      
    </attDef>
  </attList>
  
  
</classSpec>
  <classSpec ident="att.lexicographic" module="dictionaries">
  <desc>defines a set of global attributes available on elements in the base tag set for dictionaries.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  <attList>
    <attDef ident="expand" usage="opt">
      
      <desc>gives an expanded form of information presented more concisely in the dictionary</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
      
      
      
      
      
      <valDesc>any string of characters</valDesc><valDesc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</valDesc>
      
    </attDef>
    <attDef ident="norm" usage="opt">
      <gloss>normalized</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
      
      <desc>gives a normalized form of information given by the source text in a
                non-normalized form</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
      
      
      
      
      
      <valDesc>any string of characters</valDesc><valDesc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</valDesc>
      
    </attDef>
    <attDef ident="split" usage="opt">
      
      <desc>gives the list of split values for a merged form</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
      
      
      
      
      
      <valDesc>any string of characters</valDesc><valDesc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</valDesc>
      
    </attDef>
    <attDef ident="value" usage="opt">
      
      <desc>gives a value which lacks any realization in the printed source text.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
      
      
      
      
      
      <valDesc>any string of characters</valDesc><valDesc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</valDesc>
      
    </attDef>
    <attDef ident="orig" usage="opt">
      <gloss>original</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
      
      <desc>gives the original string or is the empty string when the element does not appear
                in the source text.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
      
      
      
      
      
      <valDesc>any string of characters</valDesc><valDesc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</valDesc>
      
    </attDef>
    <attDef ident="location" usage="opt">
      
      <desc>provides a reference to an <gi>anchor</gi> element elsewhere in the document
                indicating the original location of this component.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
      
      
      
      
      
      <valDesc>a valid identifier for an <gi>anchor</gi> element elsewhere in the current document.</valDesc><valDesc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</valDesc>
      
    </attDef>
    <attDef ident="mergedIn" usage="opt">
      
      <desc>gives a reference to another element, where the original appears as a merged form.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
      
      
      
      <valDesc>any valid identifier.</valDesc><valDesc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</valDesc>
      
    </attDef>
    <attDef ident="opt" usage="opt">
      <gloss>optional</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
      
      <desc>indicates whether the element is optional or not</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
      
      
      
      
      
      
    </attDef>
  </attList>
  
</classSpec>
  <elementSpec ident="superEntry" module="dictionaries">
  
  <gloss/><gloss xml:lang="pl" notBefore="2007-12-21"/>
  
  <desc>groups successive entries for a set of homographs.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="entry" module="dictionaries">
  
  <gloss/><gloss xml:lang="pl" notBefore="2007-12-21"/>
  
  <desc>contains a reasonably well-structured dictionary entry.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="entryFree" module="dictionaries">
  <gloss>unstructured entry</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
  
  
  <desc>contains a dictionary entry which does not necessarily conform to the constraints imposed
        by the <gi>entry</gi> element.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="hom" module="dictionaries">
  
  <gloss>homograph</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
  
  
  
  
  <desc>groups information relating to one homograph within an entry.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="sense" module="dictionaries">
  
  <desc>groups together all information relating to one word sense in
  a dictionary entry, for example 
    definitions, examples, and translation equivalents.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  <attList>
    <attDef ident="level" usage="opt">
      
      <desc>gives the nesting depth of this sense.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
      
      
      
      
      
      <valDesc>a positive integer</valDesc><valDesc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</valDesc>
    </attDef>
  </attList>
  
  
  
</elementSpec>
  <elementSpec ident="dictScrap" module="dictionaries">
  
  <gloss>dictionary scrap</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
  
  
  <desc>encloses a part of a dictionary entry in which other phrase-level dictionary elements are
        freely combined.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="form" module="dictionaries">
  
  <gloss>form information group</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
  
  
  
  
  <desc>groups all the information on the written and spoken forms of one headword.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  <attList>
    <attDef ident="type" usage="opt">
      
      <desc>classifies form as simple, compound, etc.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
      
      
      
      
      
      <valList type="semi">
        <valItem ident="simple">
          
          <desc>single free lexical item</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="lemma">
          
          <desc>the headword itself</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="variant">
          
          <desc>a variant form</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="compound">
          
          <desc>word formed from simple lexical items</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="derivative">
          
          <desc>word derived from headword</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="inflected">
          
          <desc>word in other than usual dictionary form</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="phrase">
          
          <desc>multiple-word lexical item</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
          
        </valItem>
      </valList>
      
    </attDef>
  </attList>
  
  
  
</elementSpec>
  <elementSpec ident="orth" module="dictionaries">
  
  <gloss>orthographic form</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
  
  
  
  
  <desc>gives the orthographic form of a dictionary headword.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  <attList>
    <attDef ident="type" usage="opt">
      
      <desc>gives the type of spelling.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
      
      
      
      
      
      <valDesc>Any convenient word or phrase, e.g. <mentioned>lat</mentioned>
                    (latinate),<mentioned>std</mentioned> (standard), <mentioned>trans</mentioned>
                (transliterated), etc.</valDesc><valDesc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</valDesc>
      
      
    </attDef>
    <attDef ident="extent" usage="opt">
      
      <desc>gives the extent of the orthographic information provided.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
      
      
      
      
      
      
      <valList type="open">
        <valItem ident="full">
          
          <gloss>full form</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
          
          
          
        </valItem>
        <valItem ident="pref">
          <gloss>prefix</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
          
          
          
        </valItem>
        <valItem ident="suff">
          
          <gloss>suffix</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
          
          
          
        </valItem>
        <valItem ident="part">
          
          <gloss>partial</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
          
          
          
        </valItem>
      </valList>
      
      
    </attDef>
  </attList>
  
  
</elementSpec>
  <elementSpec ident="pron" module="dictionaries">
  
  <gloss>pronunciation</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
  
  
  
  
  <desc>contains the pronunciation(s) of the word.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  <attList>
    <attDef ident="extent" usage="opt">
      
      <desc>indicates whether the pronunciation is for whole word or part. </desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
      
      
      
      
      
      
      <valList type="open">
        <valItem ident="full">
          
          <gloss>full form</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
          
          
          
        </valItem>
        <valItem ident="pref">
          
          <gloss>prefix</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
          
          
          
        </valItem>
        <valItem ident="suff">
          
          <gloss>suffix</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
          
          
          
        </valItem>
        <valItem ident="part">
          
          <gloss>partial</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
          
          
          
        </valItem>
      </valList>
      
      
    </attDef>
    <attDef ident="notation" usage="rwa">
      
      <desc>indicates what notation is used for the pronunciation, if more than one occurs in
                the machine-readable dictionary. </desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
      
      
      
      
      
      <valDesc>Sample values: IPA, Murray, ... </valDesc><valDesc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</valDesc>
      
      
    </attDef>
  </attList>
  
  
</elementSpec>
  <elementSpec ident="hyph" module="dictionaries">
  
  <gloss>hyphenation</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
  
  
  
  
  <desc>contains a hyphenated form of a dictionary headword, or hyphenation information in some
        other form.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="syll" module="dictionaries">
  
  <gloss>syllabification</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
  
  
  
  
  <desc>contains the syllabification of the headword.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="stress" module="dictionaries">
  
  <gloss/><gloss xml:lang="pl" notBefore="2007-12-21"/>
  
  <desc>contains the stress pattern for a dictionary headword, if given separately.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="gram" module="dictionaries">
  
  <gloss>grammatical information</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
  
  
  
  
  <desc>within an entry in a dictionary or a terminological data file, contains grammatical
        information relating to a term, word, or form.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  <attList>
    <attDef ident="type" usage="opt">
      
      <desc>classifies the grammatical information given according to
      some conve
nient typology
                — in the case of terminological information, preferably the dictionary of
                data element types specified in ISO WD 12 620.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
      
      
      
      
      
      <valList type="open">
        <valItem ident="pos">
          <gloss>part of speech</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
          
          
          
          <desc>any of the word classes to which a word may be
          assigned in a given language, based on form, meaning, or a
          combination of features, e.g. noun, verb, adjective,
          etc.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="gen">
          <gloss>gender</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
          
          
          
          <desc>formal classification by which nouns and pronouns, and
          often accompanying modifiers, are grouped and inflected, or
          changed in form, so as to control certain syntactic
          relationships</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="num">
          <gloss>number</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
          
          
          
          <desc>grammatical number, e.g. singular, plural, dual, …</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="animate">
          
          <desc>animate or inanimate</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="proper">
          
          <desc>proper noun or common noun</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
          
        </valItem>
      </valList>
      
      
    </attDef>
  </attList>
  
  
  
</elementSpec>
  <elementSpec ident="gen" module="dictionaries">
  
  <gloss>gender</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
  
  
  
  
  <desc>identifies the morphological gender of a lexical item, as given in the dictionary.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="number" module="dictionaries">
  
  <gloss/><gloss xml:lang="pl" notBefore="2007-12-21"/>
  
  <desc>indicates grammatical number associated with a form, as given in a dictionary.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="case" module="dictionaries">
  
  <gloss/><gloss xml:lang="pl" notBefore="2007-12-21"/>
  
  <desc>contains grammatical case information given by a dictionary for a given form.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="per" module="dictionaries">
  
  <gloss>person</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
  
  
  
  
  <desc>contains an indication of the grammatical person (1st, 2nd, 3rd, etc.) associated with a
    given inflected form in a dictionary.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="tns" module="dictionaries">
  
  <gloss>tense</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
  
  
  
  
  <desc>indicates the grammatical tense associated with a given inflected form in a dictionary.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="mood" module="dictionaries">
  
  <gloss/><gloss xml:lang="pl" notBefore="2007-12-21"/>
  
  <desc>contains information about the grammatical mood of verbs (e.g. indicative, subjunctive,
    imperative).</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="iType" module="dictionaries">
  
  <gloss>inflectional class</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
  
  
  
  
  <desc>indicates the inflectional class associated with a lexical item.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  <attList>
    <attDef ident="type" usage="opt">
      
      <desc>indicates the type of indicator used to specify the inflection class, when it is
                necessary to distinguish between the usual abbreviated indications (e.g.
                    <mentioned>inv</mentioned>) and other kinds of indicators, such as special codes
                referring to conjugation patterns, etc.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
      
      
      
      
      
      <valList type="open">
        <valItem ident="abbrev">
          
          <desc>abbreviated indicator</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="verbTable">
          
          <desc>coded reference to a table of verbs</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
          
        </valItem>
      </valList>
      
      
    </attDef>
  </attList>
  
  
  
</elementSpec>
  <classSpec ident="model.morphLike" module="dictionaries">
  <desc>groups elements which provide morphological information within
  a  dictionary entry.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
</classSpec>
  <classSpec ident="model.gramPart" module="dictionaries">
  <desc>groups elements allowed within a <gi>gramGrp</gi> element in a dictionary.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
</classSpec>
  <classSpec ident="model.formPart" module="dictionaries">
  <desc>groups elements allowed within a <gi>form</gi> element in a dictionary.</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
</classSpec>
  <elementSpec ident="gramGrp" module="dictionaries">
  
  <gloss>grammatical information group</gloss><gloss xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</gloss>
  
  
  
  
  <desc>groups morpho-syntactic information about a lexical item, e.g. <gi>pos</gi>, <gi>gen</gi>, <gi>number</gi>, <gi>case</gi>, or
        <gi>iType</gi> (inflectional class).</desc><desc xml:lang="pl" notBefore="2007-12-21">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="pos" module="dictionaries">
  
