<moduleSpec xmlns="http://www.tei-c.org/ns/1.0" ident="corpus">
  <elementSpec ident="textDesc" module="corpus">
  
  <gloss>text description</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  <desc>provides a description of a text in terms of its
situational parameters.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="particDesc" module="corpus">
  
  <gloss>participation description</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  <desc>describes the identifiable speakers, voices, or other participants
in a linguistic interaction. </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="settingDesc" module="corpus">
  
  <gloss>setting description</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  <desc>describes the setting or settings within which a language
interaction takes place, either as a prose description or as a
series of setting elements.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="channel" module="corpus">
  
  <gloss>primary channel</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  <desc>describes the medium or channel by which a text is delivered or
experienced. For a written text, this might be print, manuscript, e-mail, etc.;
for a spoken one, radio, telephone, face-to-face, etc.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  <attList>
    <attDef ident="mode" usage="opt">
      
      <desc>specifies the mode of this channel with respect to speech and
         writing.
   </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      <valList type="closed">
        <valItem ident="s">
          <gloss>spoken</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
          
          
          
          
        </valItem>
        <valItem ident="w">
          <gloss>written</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
          
          
          
          
        </valItem>
        <valItem ident="sw">
          <gloss>spoken to be written</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
          
          
          
          
          <desc>e.g. dictation</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="ws">
          <gloss>written to be spoken</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
          
          
          
          
          <desc>e.g. a script</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="m">
          <gloss>mixed</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
          
          
          
          
        </valItem>
        <valItem ident="x">
          <gloss>unknown or inapplicable</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
          
          
          
          
        </valItem>
      </valList>
    </attDef>
  </attList>
  
  
</elementSpec>
  <elementSpec ident="constitution" module="corpus">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  
  <desc>describes the internal composition of a text or text sample,
for example
as fragmentary, complete, etc.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  <attList>
    <attDef ident="type" usage="opt">
      
      <desc>specifies how the text was constituted.
   </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      <valList type="closed">
        <valItem ident="single">
          
          <desc>a single complete text
    </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
        </valItem>
        <valItem ident="composite">
          
          <desc>a text made by combining several smaller
             items, each individually complete
    </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
        </valItem>
        <valItem ident="frags">
          <gloss>fragments</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
          
          
          
          <desc>a text made by combining several smaller, not
             necessarily complete, items
    </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
        </valItem>
        <valItem ident="unknown">
          
          <desc>composition unknown or unspecified
   </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
        </valItem>
      </valList>
    </attDef>
  </attList>
  
  
  
</elementSpec>
  <elementSpec ident="derivation" module="corpus">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  
  <desc>describes the nature and extent of originality of this text.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  <attList>
    <attDef ident="type" usage="opt">
      
      <desc>categorizes the derivation of the text.
   </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      <valList type="open">
        <valItem ident="original">
          
          <desc>text is original
        </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="revision">
          
          <desc>text is a revision of some other text
        </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="translation">
          
          <desc>text is a translation of some other text
        </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="abridgment">
          
          <desc>text is an abridged version of some other text
        </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="plagiarism">
          
          <desc>text is plagiarized from some other text
        </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="traditional">
          
          <desc>text has no obvious source but is one of a
        number derived from some common ancestor
    </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
      </valList>
    </attDef>
  </attList>
  
  
  
</elementSpec>
  <elementSpec ident="domain" module="corpus">
  <gloss>domain of use</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  <desc>describes the most important social context in which the text was
realized or for which it is intended, for example private vs. public,
education, religion, etc.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  <attList>
    <attDef ident="type" usage="opt">
      
      <desc>categorizes the domain of use.
   </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      <valList type="open">
        <valItem ident="art">
          
          <desc>art and entertainment
    </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="domestic">
          
          <desc>domestic and private
    </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="religious">
          
          <desc>religious and ceremonial
    </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="business">
          
          <desc>business and work place
    </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="education">
          
          <desc>education
    </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="govt">
          <gloss>government</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
          
          
          
          <desc>government and law
    </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="public">
          
          <desc>other forms of public context
   </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
      </valList>
    </attDef>
  </attList>
  
  
  
</elementSpec>
  <elementSpec ident="factuality" module="corpus">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  
  <desc>describes the extent to which the text may be regarded as
imaginative or non-imaginative, that is, as describing a fictional
or a non-fictional world. </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  <attList>
    <attDef ident="type" usage="opt">
      
      <desc>categorizes the factuality of the text.
   </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      <valList type="closed">
        <valItem ident="fiction">
          
          <desc>the text is to be regarded as entirely imaginative
  </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
        </valItem>
        <valItem ident="fact">
          
          <desc>the text is to be regarded as entirely informative or factual
  </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
        </valItem>
        <valItem ident="mixed">
          
          <desc>the text contains a mixture of fact and fiction
  </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
        </valItem>
        <valItem ident="inapplicable">
          
          <desc>the fiction/fact distinction is not regarded
    as helpful or appropriate to this text
   </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
        </valItem>
      </valList>
    </attDef>
  </attList>
  
  
  
  
</elementSpec>
  <elementSpec ident="interaction" module="corpus">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  
  <desc>describes the extent, cardinality and nature of any interaction
among those producing and experiencing the text, for example in the
form of response or interjection, commentary etc.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  <attList>
    <attDef ident="type">
      
      
      <desc>specifies the degree of interaction between
  active and passive participants in the text. </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      <valList type="closed">
        <valItem ident="none">
          
          <desc>no interaction of any kind, e.g. a monologue </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
        </valItem>
        <valItem ident="partial">
          
          <desc>some degree of interaction, e.g. a monologue with set responses </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
        </valItem>
        <valItem ident="complete">
          
          <desc>complete interaction, e.g. a face to face conversation </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
        </valItem>
        <valItem ident="inapplicable">
          
          <desc>this parameter is inappropriate or inapplicable in this case </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
        </valItem>
      </valList>
    </attDef>
    <attDef ident="active" usage="opt">
      
      <desc>specifies the number of active participants
   (or <term>addressors</term>) producing parts of the text. </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      <valList type="semi">
        <valItem ident="singular">
          
          <desc>a single addressor </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="plural">
          
          <desc>many addressors </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="corporate">
          
          <desc>a corporate addressor </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="unknown">
          
          <desc>number of addressors unknown or unspecifiable </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
      </valList>
    </attDef>
    <attDef ident="passive" usage="opt">
      
      <desc>specifies the number of passive participants
   (or <term>addressees</term>) to whom a text is directed
   or in whose presence it is created or performed.
   </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      <valList type="semi">
        <valItem ident="self">
          
          <desc>text is addressed to the originator e.g. a diary </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="single">
          
          <desc>text is addressed to one other person e.g. a personal letter </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="many">
          
          <desc>text is addressed to a countable number of others
    e.g. a conversation in which all participants are identified </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="group">
          
          <desc>text is addressed to an undefined but fixed
    number of participants  e.g. a lecture </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="world">
          
          <desc>text is addressed to an undefined and indeterminately
    large number e.g. a published book </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
      </valList>
    </attDef>
  </attList>
  
  
  
</elementSpec>
  <elementSpec ident="preparedness" module="corpus">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  
  <desc>describes the extent to which a text may be regarded as
prepared or spontaneous. </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  <attList>
    <attDef ident="type" usage="opt">
      
      <desc>a keyword characterizing the type of preparedness.
   </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      <valList type="open">
        <valItem ident="none">
          
          <desc>spontaneous or unprepared
        </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="scripted">
          
          <desc>follows a script
        </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="formulaic">
          
          <desc>follows a predefined set of conventions
        </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="revised">
          
          <desc>polished or revised before presentation
   </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
      </valList>
    </attDef>
  </attList>
  
  
</elementSpec>
  <elementSpec ident="purpose" module="corpus">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  <desc>characterizes a single purpose or communicative function of the
text.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  <attList>
    <attDef ident="type" usage="opt">
      
      <desc>specifies a particular kind of purpose.
   </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      <valList type="semi">
        <valItem ident="persuade">
          
          <desc>didactic, advertising, propaganda, etc.
  </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="express">
          
          <desc>self expression, confessional, etc.
  </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="inform">
          
          <desc>convey information, educate, etc.
  </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="entertain">
          
          <desc>amuse, entertain, etc.
   </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
      </valList>
    </attDef>
    <attDef ident="degree" usage="opt">
      
      <desc>specifies the extent to which this purpose predominates.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      
    </attDef>
  </attList>
  
  
  
</elementSpec>
  <elementSpec ident="setting" module="corpus">
  
  <desc>describes one particular setting in which a language
  interaction takes place.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="locale" module="corpus">
  
  
  
  <desc>contains a brief informal description of the nature of a
place for example a room, a restaurant, a park bench etc.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="activity" module="corpus">
  
  <desc>contains a brief informal description of what a participant in a
language interaction is doing other than speaking, if anything.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
</elementSpec>
</moduleSpec>
