<moduleSpec xmlns="http://www.tei-c.org/ns/1.0" ident="drama">
  <elementSpec ident="set" module="drama">
  <gloss>setting</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  <desc>contains a description of the setting, time, locale, appearance,
etc., of the action of a play, typically found in the front matter of
a printed performance text (not a stage direction).</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="prologue" module="drama">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  
  <desc>contains the prologue to a drama,  typically spoken by an actor out of
character, possibly in association with a particular performance or venue.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="epilogue" module="drama">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  
  <desc>contains the epilogue to a drama,  typically spoken by an actor
out of character, possibly in association with a particular performance
or venue.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="performance" module="drama">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  
  <desc>contains a section of front or back matter describing how a
dramatic piece is to be performed in general or how it was performed
on some specific occasion. </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="castList" module="drama">
  
  <gloss>cast list</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  <desc>contains a single cast list or dramatis personae.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="castGroup" module="drama">
  
  <gloss>cast list grouping</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  <desc>groups one or more individual castItem
elements within a cast list.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="castItem" module="drama">
  <gloss>cast list item</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  <desc>contains a single entry within a cast list, describing
either a single role or a list of non-speaking roles.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  <attList>
    <attDef ident="type" usage="opt">
      
      <desc>characterizes the cast item.
   </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      <valList type="closed">
        <valItem ident="role">
          
          <desc>the item describes a single role.
       </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
        </valItem>
        <valItem ident="list">
          
          <desc>the item describes a list of non-speaking roles.
   </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
        </valItem>
      </valList>
    </attDef>
  </attList>
  
  
  
</elementSpec>
  <elementSpec ident="role" module="drama">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  
  <desc>the name of a dramatic role, as given in a cast list.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="roleDesc" module="drama">
  
  <gloss>role description</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  <desc>describes a character's role in a drama.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="actor" module="drama">
  
  <desc>Name of an actor appearing within a cast list.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="move" module="drama">
  
  <gloss>movement</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  <desc>marks the actual entrance or exit of one or more characters on stage.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  <attList>
    <attDef ident="type" usage="opt">
      
      <desc>characterizes the movement, for example as an entrance or exit.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      <valList type="semi">
        <valItem ident="entrance">
          
          <desc>character is entering the stage.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="exit">
          
          <desc>character is exiting the stage.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="onStage">
          
          <desc>character moves on stage</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
      </valList>
    </attDef>
    <attDef ident="where" usage="opt">
      
      <desc>specifies the direction of a stage movement.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      <valList type="open">
        <valItem ident="L">
          <gloss>left</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
          
          
          <desc>stage left</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="R">
          <gloss>right</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
          
          
          <desc>stage right</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="C">
          <gloss>center</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
          
          
          <desc>centre stage</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
      </valList>
      
    </attDef>
    <attDef ident="perf" usage="opt">
      <gloss>performance</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
      
      <desc>identifies the performance or performances in which this movement
		occurred as specified.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      <valDesc>The references are derived from the <att>xml:id</att> attribute
		on a <gi>performance</gi> element.</valDesc><valDesc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</valDesc>
    </attDef>
  </attList>
  
  
</elementSpec>
  <elementSpec ident="view" module="drama">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  
  <desc>describes the visual context of some part of a screen play in
terms of what the spectator sees, generally independent of any
dialogue.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="camera" module="drama">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  
  <desc>describes a particular camera angle or viewpoint in a screen play.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="sound" module="drama">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  
  <desc>describes a sound effect or musical sequence specified within a
screen play or radio script.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  <attList>
    <attDef ident="type" usage="opt">
      
      <desc>categorizes the sound in some respect, e.g. as music,
      special effect, etc.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
    </attDef>
    <attDef ident="discrete" usage="opt">
      
      <desc>indicates whether the sound overlaps the surrounding
      speeches or interrupts them.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      
    </attDef>
  </attList>
  
  
  
</elementSpec>
  <elementSpec ident="caption" module="drama">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  
  <desc>contains the text of a caption or other text displayed as part of
a film script or screenplay.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="tech" module="drama">
  
  <gloss>technical stage direction</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  <desc>describes a special-purpose stage direction that is not
meant for the actors.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  <attList>
    <attDef ident="type" usage="opt">
      
      <desc>categorizes the technical stage direction.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      <valList type="closed">
        <valItem ident="light">
          
          <desc>a lighting cue</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
        </valItem>
        <valItem ident="sound">
          
          <desc>a sound cue</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
        </valItem>
        <valItem ident="prop">
          
          <desc>a prop cue</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
        </valItem>
        <valItem ident="block">
          
          <desc>a blocking instruction</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
        </valItem>
      </valList>
    </attDef>
    <attDef ident="perf" usage="opt">
      <gloss>performance</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
      
      <desc>identifies the performance or performances to which this
technical direction applies.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      <valDesc>The IDREFS are derived from the <att>xml:id</att> attribute on a <gi>performance</gi> element.</valDesc><valDesc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</valDesc>
    </attDef>
  </attList>
  
  
  
</elementSpec>
</moduleSpec>
