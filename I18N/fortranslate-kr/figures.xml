<moduleSpec xmlns="http://www.tei-c.org/ns/1.0" ident="figures">
  <elementSpec ident="table" module="figures">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  
  <desc>contains text displayed in tabular form, in
rows and columns.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  <attList>
    <attDef ident="rows" usage="opt">
      
      <desc>indicates the number of rows in the table.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      <valDesc>If no number is supplied, an application must calculate the
number of rows.</valDesc><valDesc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</valDesc>
      
    </attDef>
    <attDef ident="cols" usage="opt">
      <gloss>columns</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
      
      <desc>indicates the number of columns in each row of the table.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      <valDesc>If no number is supplied, an application must calculate the
number of columns.</valDesc><valDesc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</valDesc>
      
    </attDef>
  </attList>
  
  
  
</elementSpec>
  <elementSpec ident="row" module="figures">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  
  <desc>contains one row of a table. </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="cell" module="figures">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  
  <desc>contains one cell of a table. </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="formula" module="figures">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  
  <desc>contains a mathematical or other formula.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  <attList>
    <attDef ident="notation" usage="opt">
      
      <desc>supplies the name of a previously defined notation used for the
content of the
element.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      <valDesc>The
name of a formal notation previously declared in the document type declaration.</valDesc><valDesc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</valDesc>
    </attDef>
  </attList>
  
  
  
</elementSpec>
  <elementSpec ident="figure" module="figures">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  
  <desc>groups elements representing or containing graphic information
  such as an illustration or figure.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="figDesc" module="figures">
  <gloss>description of figure</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  <desc>contains a brief prose description of the appearance or content
of a graphic figure, for use when documenting an image without
displaying it.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
</elementSpec>
</moduleSpec>
