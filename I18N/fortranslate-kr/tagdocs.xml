  <attList>
    <attDef ident="scheme" usage="opt">
      
      <desc>supplies the name of the scheme in which this name is defined.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      
      <valList type="open">
        <valItem ident="TEI">
          <gloss>text encoding initiative</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
          
          
          
          <desc>this element is part of the TEI scheme.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="DBK">
          <gloss>docbook</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
          
          
          
          <desc>this element is  part of the Docbook scheme.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="XX">
          <gloss>unknown</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
          
          
          
          <desc>this element is  part of an unknown scheme.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
      </valList>
    </attDef>
  </attList>
  
  
</elementSpec>
  <elementSpec ident="ident" module="tagdocs">
  <gloss>identifier</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  <desc>contains an identifier or name for an object of some kind in a formal language.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="tag" module="tagdocs">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  
  <desc>contains text of a complete start- or end-tag, possibly
including attribute specifications, but excluding the opening and
