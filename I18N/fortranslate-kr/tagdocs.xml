  <attList>
    <attDef ident="scheme" usage="opt">
      
      <desc>supplies the name of the scheme in which this name is defined.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      <valList type="closed">
        <valItem ident="TEI">
          <gloss>text encoding initiative</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
          
          
          
          <desc>this tag is part of the TEI scheme.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
        </valItem>
        <valItem ident="DBK">
          <gloss>docbook</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
          
          
          
          <desc>this tag is part of the Docbook scheme.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
        </valItem>
        <valItem ident="XX">
          <gloss>unknown</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
          
          <desc>this tag is part of an unknown scheme.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
      </valList>
    </attDef>
  </attList>
  
  
</elementSpec>
  <elementSpec ident="val" module="tagdocs">
  
  <gloss>value</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  <desc>contains a single attribute value.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="specList" module="tagdocs">
  <gloss>specification list</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  <desc>marks where a list of descriptions is to be inserted into the prose documentation.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="specDesc" module="tagdocs">
  <gloss>specification description</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  <desc>indicates that a description of the specified element or class
  should be included at this point within a document.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  <attList>
    <attDef ident="key" usage="opt">
      <gloss>identifier</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
      
      
      
      <desc>supplies the identifier of the documentary element or class for
      which a description is to be obtained.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      
    </attDef>
    <attDef ident="atts" usage="rec">
      <gloss>attributes</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
      
      
      
      <desc>supplies attribute names for which descriptions should
      additionally be obtained.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      <valDesc>a whitespace-separated list of attribute
      names</valDesc><valDesc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</valDesc>
      
      
    </attDef>
  </attList>
  
  
</elementSpec>
  <elementSpec xmlns:rng="http://relaxng.org/ns/structure/1.0" ident="moduleRef" module="tagdocs">
  <gloss>module reference</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  <desc>references a module which is to be incorporated into a schema.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  <attList>
    <attList org="choice">
      <attDef ident="key" usage="opt">
        <desc>the name of a TEI module</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
        
        
        
      </attDef>
      <attDef ident="url" usage="opt">
        <gloss>uniform resource locator</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
        
        <desc>refers to a non-TEI module of RELAX NG code by external location </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
        
        
        
      </attDef>
    </attList>
  </attList>
  
  
</elementSpec>
  <elementSpec ident="moduleSpec" module="tagdocs">
  <gloss>module specification</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  <desc>documents the structure, content, and purpose of a single
module, i.e. a named and externally visible group of declarations.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  <attList>
    <attDef ident="type" usage="opt">
      
      <desc>type of module to be generated</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      <valDesc>A closed set of keywords yet to be defined</valDesc><valDesc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</valDesc>
    </attDef>
  </attList>
  
</elementSpec>
  <elementSpec xmlns:rng="http://relaxng.org/ns/structure/1.0" ident="schemaSpec" module="tagdocs">
  <gloss>schema specification</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  <desc>generates a TEI-conformant schema and documentation for it.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  <attList>
    <attDef ident="start" usage="opt">
      
      <desc>specifies entry points to the schema, i.e. which elements
      are allowed to be used as the root of documents conforming to
      it. </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      
    </attDef>
    <attDef ident="ns" usage="opt">
      <gloss>namespace</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
      
      <desc>specifies the default namespace (if any) applicable to
      components of the schema.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      
    </attDef>
    <attDef ident="prefix" usage="opt">
      
      <desc>specifies a prefix which will be appended to all patterns
      relating to TEI elements. This allows for external schemas to be mixed in
      which have elements of the same names as the TEI.
      </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      
    </attDef>
    <attDef ident="targetLang" usage="opt">
      <gloss>target language</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
      
      <desc>Where names for element or attribute are available in more
      than one language, specifies which language to use when creating
      the objects in a schema.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
    </attDef>
    <attDef ident="docLang" usage="opt">
      <gloss>documentation language</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
      
      <desc>Where the description for an element, attribute, class or macro
      is available in more than one language, specifies which languages to
      use when creating documentation.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
    </attDef>
  </attList>
  
</elementSpec>
  <elementSpec ident="specGrp" module="tagdocs">
  
  <gloss>specification group</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  <desc>contains any convenient grouping of specifications for use within
  the current module.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec xmlns:rng="http://relaxng.org/ns/structure/1.0" ident="specGrpRef" module="tagdocs">
  
  <gloss>reference to a specification group</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  <desc>indicates that the  declarations contained by the
  <gi>specGrp</gi> referenced should be inserted at this point.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  <attList>
    <attDef ident="target" usage="req">
      <desc>points at the specification group  which logically belongs here.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
    </attDef>
  </attList>
  
  
</elementSpec>
  <elementSpec ident="stringVal" module="tagdocs">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  
  <desc>contains the intended expansion for the entity documented
by a <gi>macroSpec</gi> element, enclosed by quotation marks.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="elementSpec" module="tagdocs">
  <gloss>element specification</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  <desc>documents the structure, content, and purpose of a single
element type.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  <attList>
    <attDef ident="ns" usage="opt">
      
      <gloss>namespace</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
      
      
      
      <desc>specifies the namespace to which this element belongs</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      
    </attDef>
    <attDef ident="usage" usage="opt">
      
      <desc>specifies the optionality of an attribute or element. </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      <valList type="closed">
        <valItem ident="req">
          
          <gloss>required</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
          
          
          
          
        </valItem>
        <valItem ident="mwa">
          
          <gloss>mandatory when applicable</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
          
          
          
          
        </valItem>
        <valItem ident="rec">
          
          <gloss>recommended</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
          
          
          
          
        </valItem>
        <valItem ident="rwa">
          
          <gloss>recommended when applicable</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
          
          
          
          
        </valItem>
        <valItem ident="opt">
          
          <gloss>optional</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
          
          
          
          
        </valItem>
      </valList>
    </attDef>
  </attList>
  
  
</elementSpec>
  <elementSpec xmlns:rng="http://relaxng.org/ns/structure/1.0" ident="classSpec" module="tagdocs">
  <gloss>class specification</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  <desc>contains reference information for a TEI element class; 
that is a group of 
  elements which appear together in  content models, or 
  which share some common attribute, or both.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  <attList>
    <attDef ident="type" usage="req">
      
      <desc>indicates whether this is a model class or an attribute class</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      <valList type="closed">
        <valItem ident="model">
          <gloss>content model</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
          
          <desc>members of this class appear in the same  content models</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
          
          
        </valItem>
        <valItem ident="atts">
          <gloss>attributes</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
          
          <desc>members of this class share common attributes</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
          
          
        </valItem>
      </valList>
    </attDef>
    <attDef ident="generate" usage="opt">
      <desc>indicates which alternation and sequence instantiations
	of a model class may be referenced. By default, all variations
	are permitted.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      <valList type="closed">
        <valItem ident="alternation">
          <desc>members of the class are alternatives</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
          
        </valItem>
        <valItem ident="sequence">
          <desc>members of the class are to be provided in sequence</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
          
        </valItem>
        <valItem ident="sequenceOptional">
          <desc>members of the class may be provided, in sequence,
	    but are optional</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
          
        </valItem>
        <valItem ident="sequenceOptionalRepeatable">
          <desc>members of the class may be provided one or more
	    times, in sequence, but are optional. </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
          
        </valItem>
        <valItem ident="sequenceRepeatable">
          <desc>members of the class may be provided one or more times, in sequence</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
          
        </valItem>
      </valList>
    </attDef>
  </attList>
  
</elementSpec>
  <elementSpec ident="macroSpec" module="tagdocs">
  <gloss>macro specification</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  <desc>documents the function and implementation of a pattern. </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  <attList>
    <attDef ident="type" usage="opt">
      
      <desc>indicates which type of entity should be generated, when an ODD
	processor is generating a module using XML DTD syntax.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      <valList type="closed">
        <valItem ident="pe">
          
          <gloss>parameter entity</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
          
          
          
          
        </valItem>
        <valItem ident="dt">
          
          <gloss>datatype entity</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
          
          
          
          
        </valItem>
      </valList>
    </attDef>
  </attList>
</elementSpec>
  <elementSpec ident="remarks" module="tagdocs">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  
  <desc>contains any commentary or discussion about the usage of an
element, attribute, class, or entity not otherwise documented within the
containing element.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="listRef" module="tagdocs">
  
  <gloss>list of references</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  <desc>supplies a list of significant references to places where this
  element is discussed, in the current document or elsewhere.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="exemplum" module="tagdocs">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  
  <desc>contains a single example demonstrating the use of an element,
  together with optional paragraphs of commentary.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="classes" module="tagdocs">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  
  <desc>specifies all the classes of which the documented element or
class is a member or subclass.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  <attList>
    <attDef ident="mode" usage="opt">
      
      <desc>specifies the effect of this declaration on its parent
    module.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      <valList type="closed">
        <valItem ident="change">
          <desc>this declaration changes the declaration of the same
    name in the current definition</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
        </valItem>
        <valItem ident="replace">
          <desc>this declaration replaces the declaration of the same
    name in the current definition</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
        </valItem>
      </valList>
    </attDef>
  </attList>
  
  
  
</elementSpec>
  <elementSpec ident="memberOf" module="tagdocs">
  
  <desc>specifies  class membership of the parent element or class.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  <attList>
    <attDef ident="key">
      <desc>specifies the identifier for a class of which the documented
 element or class is a member or subclass</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
    </attDef>
    <attDef ident="mode" usage="opt">
      
      <desc>specifies the effect of this declaration on its parent
    module.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      <valList type="closed">
        <valItem ident="add">
          <desc>this declaration is added to the current definitions</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
        </valItem>
        <valItem ident="delete">
          <desc>this declaration and all of its children are removed from the current setup</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
        </valItem>
      </valList>
    </attDef>
