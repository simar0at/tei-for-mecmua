    </attDef>
  </attList>
</elementSpec>
  <elementSpec ident="eg" module="tagdocs">
  <gloss>example</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
