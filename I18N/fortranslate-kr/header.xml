<moduleSpec xmlns="http://www.tei-c.org/ns/1.0" ident="header">
  <classSpec ident="model.headerPart" module="header">
  <desc>groups high level elements which may appear more than once in a TEI Header. </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
</classSpec>
  <elementSpec ident="teiHeader" module="header">
  
  <gloss>TEI Header</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>supplies the descriptive and declarative information making
	up an electronic title page prefixed to every TEI-conformant
	text.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  <attList>
    <attDef ident="type" usage="opt">
      
      <desc>specifies the kind of document to which the header is
      attached, for example whether it is a corpus or individual text.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      
      
      
      <valList type="open">
        <valItem ident="text">
          
          <desc>the header is attached to a single text.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
        </valItem>
        <valItem ident="corpus">
          
          <desc>the header is attached to a corpus.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
        </valItem>
      </valList>
    </attDef>
    
  </attList>
  
  
  
</elementSpec>
  <elementSpec ident="fileDesc" module="header">
  
  <gloss>file description</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>contains a full bibliographic description of an electronic
file.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="titleStmt" module="header">
  
  <gloss>title statement</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>groups information about the title of a work and those
responsible for its intellectual content.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="sponsor" module="header">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  
  <desc>specifies the name of a sponsoring organization or
institution.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="funder" module="header">
  
  <gloss>funding body</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>specifies the name of an individual, institution, or organization
responsible for the funding of a project or text.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="principal" module="header">
  
  <gloss>principal researcher</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>supplies the name of the principal researcher responsible for the
creation of an electronic text.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="editionStmt" module="header">
  
  <gloss>edition statement</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>groups information relating to one edition of a text.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="edition" module="header">
  
  <gloss>edition</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>describes the particularities of one edition of a text.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="extent" module="header">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  
  <desc>describes the approximate size of a text as stored on
some carrier medium, whether digital or non-digital, specified in any convenient units.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="publicationStmt" module="header">
  
  <gloss>publication statement</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>groups information concerning the publication or distribution
of an electronic or other text. </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="distributor" module="header">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  
  <desc>supplies the name of a person or other agency responsible for the
distribution of a text.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="authority" module="header">
  
  <gloss>release authority</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>supplies the name of a person or other agency responsible for
  making an electronic file available, other than a publisher or
  distributor.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="idno" module="header">
  
  <gloss>identifying number</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>supplies any standard or non-standard number used to identify a
bibliographic item.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  <attList>
    <attDef ident="type" usage="opt">
      
      <desc>categorizes the number, for example as an ISBN or other
         standard series.
   </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      
      
      <valDesc>A name or abbreviation indicating what type of identifying
         number is given (e.g. ISBN, LCCN).</valDesc><valDesc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</valDesc>
    </attDef>
  </attList>
  
  
</elementSpec>
  <elementSpec ident="availability" module="header">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  
  <desc>supplies information about the availability of a text, for
  example any restrictions on its use or distribution, its copyright
  status, etc.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  <attList>
    <attDef ident="status" usage="opt">
      
      <desc>supplies a code identifying the current availability of the
text.
   </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      
      
      <valList type="closed">
        <valItem ident="free">
          
          <desc>the text is freely available.
    </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
          
        </valItem>
        <valItem ident="unknown">
          
          <desc>the status of the text is unknown.
    </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
          
        </valItem>
        <valItem ident="restricted">
          
          <desc>the text is not freely available.
   </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
          
        </valItem>
      </valList>
    </attDef>
  </attList>
  
  
  
</elementSpec>
  <elementSpec ident="seriesStmt" module="header">
  
  <gloss>series statement</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>groups information about the series, if any, to which
a publication belongs.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="notesStmt" module="header">
  
  <gloss>notes statement</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>collects together any notes providing information about a text
additional to that recorded in other parts of the bibliographic
description.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <classSpec ident="model.sourceDescPart" module="header">
  <desc>groups elements which may be used inside <gi>sourceDesc</gi>
and appear multiple times.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
</classSpec>
  <elementSpec ident="sourceDesc" module="header">
  
  <gloss>source description</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  <desc>supplies a description of the source text(s) from
which an electronic text was derived or generated.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="biblFull" module="header">
  
  <gloss>fully-structured bibliographic citation</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  <desc>contains a fully-structured bibliographic citation, in which all
components of the TEI file description
are present.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
</elementSpec>
  <classSpec ident="model.encodingPart" module="header">
  <desc>groups elements which may be used inside <gi>encodingDesc</gi>
and appear multiple times.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
</classSpec>
  <elementSpec ident="encodingDesc" module="header">
  
  <gloss>encoding description</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>documents the relationship between an electronic text and the
source or sources from which it was derived.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="projectDesc" module="header">
  
  <gloss>project description</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>describes in detail the aim or purpose for which an electronic
file was encoded, together with any other relevant information
concerning the process by which it was assembled or collected.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="samplingDecl" module="header">
  
  <gloss>sampling declaration</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>contains a prose description of the rationale and methods used
in sampling texts in the creation of a corpus or collection.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <classSpec ident="model.editorialDeclPart" module="header">
  <desc>groups elements which may be used inside
  <gi>editorialDecl</gi> and appear multiple times.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
</classSpec>
  <elementSpec ident="editorialDecl" module="header">
  
  <gloss>editorial practice declaration</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>provides details of editorial principles and practices applied
during the encoding of a text.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="correction" module="header">
  
  <gloss>correction principles</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>states how and under what circumstances corrections have been
made in the text.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  <attList>
    <attDef ident="status" usage="opt">
      
      <desc>indicates the degree of correction applied to the text.
   </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      
      
      <valList type="closed">
        <valItem ident="high">
          
          <desc>the text has been thoroughly checked and
                       proofread.
    </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
          
        </valItem>
        <valItem ident="medium">
          
          <desc>the text has been checked at least once.
    </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
          
        </valItem>
        <valItem ident="low">
          
          <desc>the text has not been checked.
    </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
          
        </valItem>
        <valItem ident="unknown">
          
          <desc>the correction status of the text is unknown.
   </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
          
        </valItem>
      </valList>
    </attDef>
    <attDef ident="method" usage="opt">
      
      <desc>indicates the method adopted to indicate corrections within the
         text.
   </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      
      
      <valList type="closed">
        <valItem ident="silent">
          
          <desc>corrections have been made silently
    </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
          
        </valItem>
        <valItem ident="markup">
          
          <desc>corrections have been represented using markup</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
        </valItem>
      </valList>
    </attDef>
  </attList>
  
  
  
</elementSpec>
  <elementSpec xmlns:rng="http://relaxng.org/ns/structure/1.0" ident="normalization" module="header">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  <desc>indicates the extent of normalization or regularization of the
original source carried out in converting it to electronic form.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  <attList>
    <attDef ident="source" usage="opt">
      
      <desc>indicates the authority for any normalization carried out.
    </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      
      
      <valDesc>Points to a bibliographic
      description or other resource documenting the principles
      underlying the normalization which has been applied.
   </valDesc><valDesc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</valDesc>
    </attDef>
    <attDef ident="method" usage="opt">
      
      <desc>indicates the method adopted to indicate normalizations within
         the text.
   </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      
      
      <valList type="closed">
        <valItem ident="silent">
          
          <desc>normalization made silently
  </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
          
        </valItem>
        <valItem ident="markup">
          
          <desc>normalization represented using markup
   </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
          
        </valItem>
      </valList>
    </attDef>
  </attList>
  
  
</elementSpec>
  <elementSpec ident="quotation" module="header">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  
  <desc>specifies editorial practice adopted with respect to quotation
  marks in the original. </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  <attList>
    <attDef ident="marks" usage="opt">
      <gloss>quotation marks</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
      
      <desc>indicates whether or not quotation marks have been retained as
      content within the text. </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      
      
      <valList type="closed">
        <valItem ident="none">
          
          <desc>no quotation marks have been retained </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
          
        </valItem>
        <valItem ident="some">
          
          <desc>some quotation marks have been retained </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
          
        </valItem>
        <valItem ident="all">
          
          <desc>all quotation marks have been retained </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
          
        </valItem>
      </valList>
    </attDef>
    <attDef ident="form" usage="opt">
      
      <desc>specifies how quotation marks are indicated within the
      text.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      
      
      <valList type="closed">
        <valItem ident="data">
          
          <desc>quotation marks are retained as data.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
          
        </valItem>
        <valItem ident="rend">
          <gloss><att>rend</att> attribute</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
          
          
          
          
          <desc>the <att>rend</att> attribute is consistently used to
          indicate the form of quotation marks.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
          
        </valItem>
        <valItem ident="std">
          <gloss>standardized</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
          
          
          
          
          <desc>use of quotation marks has been standardized.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
          
        </valItem>
        <valItem ident="nonstd">
          <gloss>not standardized</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
          
          
          
          
          <desc>quotation marks are represented inconsistently.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
          
        </valItem>
        <valItem ident="unknown">
          
          <desc>use of quotation marks is unknown.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
          
        </valItem>
      </valList>
    </attDef>
  </attList>
  
  
  
</elementSpec>
  <elementSpec ident="hyphenation" module="header">
  
  
  <desc>summarizes the way in which hyphenation in a source text has been
treated in an encoded version of it.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  <attList>
    <attDef ident="eol" usage="opt">
      <gloss>end-of-line</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
      
      <desc>indicates whether or not end-of-line hyphenation has been
	retained in a text.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      
      
      <valList type="closed">
        <valItem ident="all">
          
          <desc>all end-of-line hyphenation has been retained,
                   even though the lineation of the original
                   may not have been.
   </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
          
        </valItem>
        <valItem ident="some">
          
          <desc>end-of-line hyphenation has been retained in some
                   cases.
   </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
          
        </valItem>
        <valItem ident="hard">
          
          <desc>all soft end-of-line hyphenation has been removed:
                   any remaining end-od-line hyphenation should be retained.
  </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
          
        </valItem>
        <valItem ident="none">
          
          <desc>all end-of-line hyphenation has been removed:
                  any remaining hyphenation occurred within the line.
   </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
          
          
        </valItem>
      </valList>
    </attDef>
  </attList>
  
  
</elementSpec>
  <elementSpec ident="segmentation" module="header">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  
  <desc>describes the principles according to which the text has been
segmented, for example into sentences, tone-units, graphemic strata,
etc.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="stdVals" module="header">
  
  <gloss>standard values</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>specifies the format used when standardized date or number
values are supplied.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="interpretation" module="header">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  
  <desc>describes the scope of any analytic or interpretive information
added to the text in addition to the transcription. </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="tagsDecl" module="header">
  
  <gloss>tagging declaration</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>provides detailed information about the tagging applied to a document.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="tagUsage" module="header">
  
  
  <desc>supplies information about the usage of a specific element
within a text. </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  <attList>
    <attDef ident="gi" usage="req">
      <gloss>element name</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
      
      <desc>the name (generic identifier) of the element indicated by
      the tag.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      
      
      <valDesc>the name of an element within the namespace indicated
      by the parent <gi>namespace</gi> element</valDesc><valDesc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</valDesc>
    </attDef>
    <attDef ident="occurs" usage="rec">
      
      <desc>specifies the number of occurrences of this element within the text.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      
      
      <valDesc>an integer number greater than zero</valDesc><valDesc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</valDesc>
    </attDef>
    <attDef ident="withId" usage="rec">
      <gloss>with unique identifier</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
      
      <desc>specifies the number of occurrences of this element within
      the text which bear a distinct value for the global
      <att>xml:id</att> attribute. </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      
      
      <valDesc>an integer number greater than zero</valDesc><valDesc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</valDesc>
    </attDef>
    <attDef ident="render" usage="opt">
      
      <desc>specifies the identifier of a <gi>rendition</gi> element
      which defines how this element is to be rendered.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      
      
      <valDesc>an identifier specified as the value of the
      <att>xml:id</att> attribute on some <gi>rendition</gi> element
      in the current document.</valDesc><valDesc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</valDesc>
    </attDef>
  </attList>
  
  
</elementSpec>
  <elementSpec xmlns:rng="http://relaxng.org/ns/structure/1.0" ident="namespace" module="header">
  
  <desc>supplies the formal name of the namespace to which the elements
documented by its children belong. </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  <attList>
    <attDef ident="name" usage="req">
      
      <desc>the full formal name of the namespace concerned.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      
      
    </attDef>
  </attList>
  
  
</elementSpec>
  <elementSpec ident="rendition" module="header">
  
  
  <desc>supplies information about the rendition or appearance of one or more
elements in the source text.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  <attList>
    <attDef ident="scheme">
      <desc>identifies the language used to describe the rendition.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      <valList type="closed">
        <valItem ident="css">
          <desc>Cascading Stylesheet Language</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="xslfo">
          <desc>Extensible  Stylesheet Language
Formatting Objects</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="free">
          <desc>Informal free text description</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="other">
          <desc>A user-defined rendition description language</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
      </valList>
    </attDef>
  </attList>
  
  
  
</elementSpec>
  <elementSpec ident="refsDecl" module="header">
  
  <gloss>references declaration</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>specifies how canonical references are constructed for this
  text.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="cRefPattern" module="header">
  
  <gloss>canonical reference pattern</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>specifies an expression and replacement pattern for
  tranforming a canonical reference into a URI. </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  <attList>
    <attDef ident="matchPattern" usage="req">
      
      <desc>specifies a regular expression against which the values of
      <att>cRef</att> attributes can be matched.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      
      
      <valDesc>must be a regular expression according to the <ref target="http://www.w3.org/TR/xmlschema-2/#regexs">W3C XML Schema
      Language</ref></valDesc><valDesc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</valDesc>
      
    </attDef>
    <attDef ident="replacementPattern" usage="req">
      
      <desc>specifies a <soCalled>replacement pattern</soCalled>
      which, once subpattern substitution has been performed, provides
      a URI.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      
      
      <valDesc>Should be the skeleton of a relative or absolute URI,
      with references to groups in the <att>matchPattern</att>.</valDesc><valDesc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</valDesc>
      
    </attDef>
  </attList>
  
  
  
</elementSpec>
  <elementSpec ident="refState" module="header">
  
  <gloss>reference state</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  <desc>specifies one component of a canonical reference defined by
the milestone method.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  <attList>
    <attDef ident="ed" usage="opt">
      <gloss>edition</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
      
      <desc>indicates which edition or version the milestone applies to.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      <valDesc>Any string of characters; usually a siglum conventionally used
for the edition.</valDesc><valDesc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</valDesc>
      
    </attDef>
    <attDef ident="unit" usage="req">
      
      <desc>indicates what kind of state is changing at this milestone.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      <valList type="semi">
        <valItem ident="page">
          
          <desc>page breaks in the reference edition.
    </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="column">
          
          <desc>column breaks.
    </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="line">
          
          <desc>line breaks.
    </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="book">
          
          <desc>any units termed book, liber, etc.
    </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="poem">
          
          <desc>individual poems in a collection.
    </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="canto">
          
          <desc>cantos or other major sections of a poem.
    </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="stanza">
          
          <desc>stanzas within a poem, book, or canto.
    </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="act">
          
          <desc>acts within a play.
    </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="scene">
          
          <desc>scenes within a play or act.
    </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="section">
          
          <desc>sections of any kind.
    </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="absent">
          
          <desc>passages not present in the reference edition.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
      </valList>
    </attDef>
    <attDef ident="length" usage="opt">
      
      <desc>specifies the fixed length of the reference component.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      <valDesc>Should be a positive integer; if no value is provided,
    the length is unlimited and goes to the next delimiter or to the
    end of the value.</valDesc><valDesc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</valDesc>
      
    </attDef>
    <attDef ident="delim" usage="opt">
      <gloss>delimiter</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
      
      <desc>supplies a delimiting string following the reference
          component.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      <valDesc>If a single space is used it is interpreted as
          whitespace.</valDesc><valDesc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</valDesc>
    </attDef>
  </attList>
  
  
</elementSpec>
  <elementSpec ident="classDecl" module="header">
  
  <gloss>classification declarations</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>contains one or more taxonomies defining any classificatory
codes used elsewhere in the text.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="taxonomy" module="header">
  
  <desc>defines a typology used to classify texts either implicitly, by
means of a bibliographic citation, or explicitly by a structured
taxonomy.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="category" module="header">
  
  <desc>contains an individual descriptive category, possibly nested
within a superordinate category, within a user-defined taxonomy.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="catDesc" module="header">
  
  <gloss>category description</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>describes some category within a taxonomy
or text typology, either in the form of a brief prose description
or in terms of the situational parameters used by the TEI
formal textDesc.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec xmlns:rng="http://relaxng.org/ns/structure/1.0" ident="appInfo" module="header">
  <gloss>application information</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  <desc>records information about an application which has
  edited the TEI file. </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
</elementSpec>
  <elementSpec xmlns:rng="http://relaxng.org/ns/structure/1.0" ident="application" module="header">
  <desc>provides information about an application which has acted
  upon the document.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  <attList>
    <attDef ident="ident" usage="req">
      <desc>Supplies an identifier for the application, independent of
      its version number or display name.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
    </attDef>
    <attDef ident="version" usage="req">
      <desc>Supplies a version number for the application, independent of
      its identifier or display name.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
    </attDef>
  </attList>
  
</elementSpec>
  <classSpec ident="model.applicationLike" module="header">
  <desc>groups elements used to record application-specific
  information about a document in its header.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
</classSpec>
  <classSpec ident="model.profileDescPart" module="header">
  <desc>groups elements which may be used inside <gi>profileDesc</gi>
and appear multiple times.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
</classSpec>
  <elementSpec ident="profileDesc" module="header">
  
  <gloss>text-profile description</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>provides a detailed description of non-bibliographic aspects of a
text, specifically the languages and sublanguages used, the situation in
which it was produced, the participants and their setting.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec xmlns:rng="http://relaxng.org/ns/structure/1.0" ident="handNote" module="header">
  
  <gloss>note on hand</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  <desc>describes a particular style
or hand distinguished within a manuscript.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="creation" module="header">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  
  <desc>contains information about the creation of a text.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="langUsage" module="header">
  
  <gloss>language usage</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>describes the languages, sublanguages, registers, dialects etc.
represented within a text.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="language" module="header">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  <desc>characterizes a single language or sublanguage used within a
  text.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  <attList>
    <attDef ident="ident" usage="req">
      <gloss>identifier</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
      
      <desc>Supplies a language code constructed as defined in <ref target="http://tools.ietf.org/html/bcp47">BCP 47</ref> which is used to identify the language documented
      by this element, and which is referenced by the global <att>xml:lang</att> attribute.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      
      
    </attDef>
    <attDef ident="usage" usage="opt">
      
      <desc>specifies the approximate percentage (by volume) of the
      text which uses this language.
      </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      
      
      <valDesc>a whole number between 0 and 100</valDesc><valDesc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</valDesc>
    </attDef>
  </attList>
  
  
  
</elementSpec>
  <elementSpec ident="textClass" module="header">
  
  <gloss>text classification</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>groups information which describes the nature or topic of a text
in terms of a standard classification scheme, thesaurus, etc.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec ident="keywords" module="header">
  
  
  
  <desc>contains a list of keywords or phrases identifying the topic or
nature of a text.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  <attList>
    <attDef ident="scheme" usage="req">
      
      <desc>identifies the controlled vocabulary within which the set of
  keywords concerned is defined.
   </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      
      
      <valDesc>Usually this will indicate an external website or other
      location where the scheme is documented. </valDesc><valDesc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</valDesc>
    </attDef>
  </attList>
  
  
</elementSpec>
  <elementSpec ident="classCode" module="header">
  <gloss>classification code</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  <desc>contains the classification code used for this text in some
standard classification system.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  <attList>
    <attDef ident="scheme" usage="req">
      
      <desc>identifies the classification system or taxonomy in use.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      
      
      <valDesc>may point to a local definition, for example in a
      <gi>taxonomy</gi> element, or more usually to some external
      location where the scheme is fully defined.</valDesc><valDesc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</valDesc>
    </attDef>
  </attList>
  
  
</elementSpec>
  <elementSpec ident="catRef" module="header">
  
  <gloss>category reference</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>specifies one or more defined categories
within some taxonomy or text typology.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  <attList>
    <attDef ident="target" usage="req">
      
      <desc>identifies the categories concerned
   </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      
      
      <valDesc>A series of one or more space-separated pointers (URIs)
      to <gi>category</gi> elements, typically located within a
      <gi>taxonomy</gi> element inside a TEI header</valDesc><valDesc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</valDesc>
    </attDef>
    <attDef ident="scheme" usage="opt">
      
      <desc>identifies the classification scheme within which the set of
    categories concerned is defined
   </desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      
      
      
      
      <valDesc>May supply the identifier of the associated <gi>taxonomy</gi> element.</valDesc><valDesc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</valDesc>
    </attDef>
  </attList>
  
  
  
</elementSpec>
  <elementSpec ident="revisionDesc" module="header">
  <gloss>revision description</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  
  
  
  
  
  <desc>summarizes the revision history for a file.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  
  
  
</elementSpec>
  <elementSpec xmlns:rng="http://relaxng.org/ns/structure/1.0" ident="change" module="header">
  
  <gloss/><gloss xml:lang="kr" notBefore="2007-12-20"/>
  <desc>summarizes a particular change or correction made
to a particular version of an electronic text which is
shared between several researchers.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  
  
  
  
  
  <attList>
    <attDef ident="when" usage="mwa">
      
      <desc>supplies the date of the change in standard form, i.e. YYYY-MM-DD.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      <valDesc>a date, time, or date &amp; time in any of the formats
      defined in <title>XML Schema Part 2: Datatypes Second
      Edition</title></valDesc><valDesc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</valDesc>
    </attDef>
  </attList>
  
  
  
</elementSpec>
  <elementSpec xmlns:rng="http://relaxng.org/ns/structure/1.0" ident="geoDecl" module="header">
  <gloss>geographic coordinates declaration</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
  
  <desc>documents the notation and the datum used for geographic
  coordinates expressed as content of the <gi>geo</gi> element
  elsewhere within the document.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
  
  
  
  <attList>
    <attDef ident="datum" usage="mwa">
      
      <desc>supplies a commonly used code name for the datum employed.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
      
      
      
      <valList type="semi">
        <valItem ident="WGS84">
          <gloss>World Geodetic System</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
          
          <desc>a pair of numbers to be interpreted as latitude followed by longitude
	  according to the World Geodetic System.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="MGRS">
          <gloss>Military Grid Reference System</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
          
          <desc>the values supplied are geospatial entity object codes,
	based on  Universal Transverse Mercator coordinates</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="OSGB36">
          <gloss>ordnance survey great britain</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
          
          <desc>the value supplied is to be interpreted as a British National Grid
	  Reference.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
        <valItem ident="ED50">
          <gloss>European Datum coordinate system</gloss><gloss xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</gloss>
          
          <desc>the value supplied is to be interpreted as latitude followed by
	  longitude according to the European Datum coordinate system.</desc><desc xml:lang="kr" notBefore="2007-12-20">TO BE TRANSLATED</desc>
          
        </valItem>
      </valList>
    </attDef>
  </attList>
  
</elementSpec>
</moduleSpec>
