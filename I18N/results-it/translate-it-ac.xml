<?xml version="1.0" encoding="utf-8"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
  <classSpec ident="att.xmlspace">
    <desc version="2008-04-08" xml:lang="it">raggruppa elementi TEI per i quali si ritiene utile
      specificare la gestione degli spazi bianchi usando l'attributo xml:space definito da W3C.</desc>
  </classSpec>
  <elementSpec ident="w">
    <attList>
      <attDef ident="lemmaRef">
        <desc version="2008-04-08" xml:lang="it">offre un puntatore ad una definizione della radice
          di questa forma.</desc>
      </attDef>
    </attList>
  </elementSpec>
  <elementSpec ident="tag">
    <attList>
      <attDef ident="type">
        <desc version="2008-04-08" xml:lang="it">indica di quale tipo di marcatore XML si tratta</desc>
      </attDef>
    </attList>
  </elementSpec>
  <elementSpec ident="tag">
    <attList>
      <attDef ident="type">
        <valList>
          <valItem ident="start">
            <desc version="2008-04-08" xml:lang="it">indica uno start-tag delimitato dai segni &lt; e
              &gt;</desc>
          </valItem>
        </valList>
      </attDef>
    </attList>
  </elementSpec>
  <elementSpec ident="tag">
    <attList>
      <attDef ident="type">
        <valList>
          <valItem ident="end">
            <desc version="2008-04-08" xml:lang="it">indica un end-tag delimitato dai segni &lt;/ e
              &gt;</desc>
          </valItem>
        </valList>
      </attDef>
    </attList>
  </elementSpec>
  <elementSpec ident="tag">
    <attList>
      <attDef ident="type">
        <valList>
          <valItem ident="empty">
            <desc version="2008-04-08" xml:lang="it">indica un marcatore vuoto delimitato dai segni &lt; e
              /&gt;</desc>
          </valItem>
        </valList>
      </attDef>
    </attList>
  </elementSpec>
  <elementSpec ident="tag">
    <attList>
      <attDef ident="type">
        <valList>
          <valItem ident="pi">
            <desc version="2008-04-08" xml:lang="it">indica una pi (istruzione di processing)
              delimitata dai segni &lt;? e ?&gt;</desc>
          </valItem>
        </valList>
      </attDef>
    </attList>
  </elementSpec>
  <elementSpec ident="tag">
    <attList>
      <attDef ident="type">
        <valList>
          <valItem ident="comment">
            <desc version="2008-04-08" xml:lang="it">indica un commento delimitato dai segni &lt;!--
              e --&gt;</desc>
          </valItem>
        </valList>
      </attDef>
    </attList>
  </elementSpec>
  <elementSpec ident="tag">
    <attList>
      <attDef ident="type">
        <valList>
          <valItem ident="ms">
            <desc version="2008-04-08" xml:lang="it">indica una sezione marcata, delimitata dai segni
              &lt;[CDATA[ e ]]&gt;</desc>
          </valItem>
        </valList>
      </attDef>
    </attList>
  </elementSpec>
</TEI>
