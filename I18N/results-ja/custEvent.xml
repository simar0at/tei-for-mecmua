<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="msdescription" xml:id="CUSTEVENT" ident="custEvent">
  <gloss>custodial event</gloss>

  <desc version="2008-01-31" xml:lang="ja">
  手書き資料の管理履歴における，ひとつの事象を示す．
  </desc>
  <classes>
    <memberOf key="att.datable"/>
    <memberOf key="att.typed"/>
  </classes>
  <content>
    <rng:ref xmlns:rng="http://relaxng.org/ns/structure/1.0" name="macro.specialPara"/>
  </content>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples" xmlns:rng="http://relaxng.org/ns/structure/1.0">
      <custEvent type="photography">Photographed by David Cooper on <date>12 Dec 1964</date></custEvent>
    </egXML>
  </exemplum>
  <listRef>
    <ptr target="#msadch"/>
  </listRef>
</elementSpec>
