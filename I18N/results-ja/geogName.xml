<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="namesdates" xml:id="GEOGNAME" usage="opt" ident="geogName">
  <equiv/>
  <gloss>geographical name</gloss>
  <gloss version="2008-01-31" xml:lang="ja"></gloss>

  <desc version="2006-10-28" xml:lang="ja">
  ウィンドラッシュ峡谷，シナイ山などの地理的特性に関する名前．
  </desc>
  <classes>
    <memberOf key="att.naming"/>
    <memberOf key="model.placeNamePart"/>
  </classes>
  <content>
    <rng:ref xmlns:rng="http://relaxng.org/ns/structure/1.0" name="macro.phraseSeq"/>
  </content>
  <attList>
    <attDef ident="type" usage="mwa">
      <equiv/>

      <desc version="2008-01-31" xml:lang="ja">
      当該名前を分類するための，文化的，言語的，ソフトウェア依存の情報
      を示す．
      </desc>
      <datatype>
        <rng:ref xmlns:rng="http://relaxng.org/ns/structure/1.0" name="data.enumerated"/>
      </datatype>

      <valDesc version="2008-01-31" xml:lang="ja" >
      ソフトウェアで定義されている符号のひとつ．
      </valDesc>
    </attDef>
  </attList>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples">
      <geogName>
        <geogFeat>Mount</geogFeat>
        <name>Sinai</name>
      </geogName>
    </egXML>
  </exemplum>
  <listRef>
    <ptr target="#NDPLAC"/>
  </listRef>
</elementSpec>
