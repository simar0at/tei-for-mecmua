<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="tagdocs" xml:id="LISTREF" ident="listRef">
  <equiv/>
  <gloss>list of references</gloss>
  <gloss version="2008-01-31" xml:lang="ja"></gloss>

  <desc version="2008-01-31" xml:lang="ja">
  場所への参照のリストを示す．当該文書中でも外部文書でもよい．
  </desc>

  <classes>
    <memberOf key="model.oddDecl"/>
  </classes>
  <content>
    <rng:oneOrMore xmlns:rng="http://relaxng.org/ns/structure/1.0">
      <rng:ref name="ptr"/>
    </rng:oneOrMore>
  </content>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples">
      <listRef>
        <ptr target="#ddc12"/>
      </listRef>
    </egXML>
  </exemplum>
</elementSpec>
