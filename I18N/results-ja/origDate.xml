<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="msdescription" xml:id="ORIGDATE" ident="origDate">
  <gloss>origin date</gloss>

  <desc version="2008-01-31" xml:lang="ja">
  手書き資料が生まれた日付を特定するたための日付を示す．
  </desc>

  <classes>
    <memberOf key="att.datable"/>
    <memberOf key="att.editLike"/>
    <memberOf key="model.pPart.msdesc"/>
  </classes>
  <content>
    <rng:text xmlns:rng="http://relaxng.org/ns/structure/1.0"/>
  </content>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples" xmlns:rng="http://relaxng.org/ns/structure/1.0">
      <origDate notBefore="-0300" notAfter="-0200">3rd century BCE</origDate>
    </egXML>
  </exemplum>
  <listRef>
    <ptr target="#msdates"/>
  </listRef>
</elementSpec>
