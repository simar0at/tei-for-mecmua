<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="dictionaries" xml:id="USG" usage="opt" ident="usg">
  <equiv/>
  <gloss>usage</gloss>
  <gloss version="2008-01-31" xml:lang="ja"></gloss>


  <desc version="2008-01-31" xml:lang="ja">
  辞書項目中にある用例情報を示す．
  </desc>

  <classes>
    <memberOf key="model.entryPart.top"/>
    <memberOf key="model.entryPart"/>
    <memberOf key="model.gramPart"/>
    <memberOf key="att.lexicographic"/>
  </classes>
  <content>
    <rng:ref xmlns:rng="http://relaxng.org/ns/structure/1.0" name="macro.paraContent"/>
  </content>
  <attList>
    <attDef ident="type" usage="opt">
      <equiv/>


      <desc version="2008-01-31" xml:lang="ja">
      当該用例情報を分類する．
      </desc>

      <datatype>
        <rng:ref xmlns:rng="http://relaxng.org/ns/structure/1.0" name="data.enumerated"/>
      </datatype>
      <valList type="open">
        <valItem ident="geo">
          <gloss>geographic</gloss>

          <desc version="2008-01-31" xml:lang="ja" >
          地理的範囲．
          </desc>

        </valItem>
        <valItem ident="time">
          <equiv/>

          <desc version="2008-01-31" xml:lang="ja" >
          時代(古代，昔，など)．
          </desc>

        </valItem>
        <valItem ident="dom">
          <equiv/>
          <gloss>domain</gloss>


          <desc version="2008-01-31" xml:lang="ja" >
          分野，主題(科学，文学，など)．
          </desc>

        </valItem>
        <valItem ident="reg">
          <equiv/>
          <gloss>register</gloss>
          <desc/>

        </valItem>
        <valItem ident="style">
          <equiv/>


          <desc version="2008-01-31" xml:lang="ja" >
          スタイル(比喩的，字義的，など)．
          </desc>

        </valItem>
        <valItem ident="plev">
          <gloss>preference level</gloss>


          <desc version="2008-01-31" xml:lang="ja" >
          使用度(主たる，通常，など)．
          </desc>

        </valItem>
        <valItem ident="lang">
          <gloss>language</gloss>

          <desc version="2008-01-31" xml:lang="ja" >
          語源や他の言語学的解説に出てくる言語の名前．
          </desc>
        
        </valItem>
        <valItem ident="gram">
          <gloss>grammatical</gloss>

          <desc version="2008-01-31" xml:lang="ja" >
          文法上の用法．
          </desc>

        </valItem>
        <valItem ident="syn">
          <gloss>synonym</gloss>

          <desc version="2008-01-31" xml:lang="ja" >
          同義語．
          </desc>

        </valItem>
        <valItem ident="hyper">
          <gloss>hypernym</gloss>

          <desc version="2008-01-31" xml:lang="ja" >
          上位語．
          </desc>

        </valItem>
        <valItem ident="colloc">
          <gloss>collocation</gloss>

          <desc version="2008-01-31" xml:lang="ja" >
          見出し語の連語関係．
          </desc>

        </valItem>
        <valItem ident="comp">
          <gloss>complement</gloss>

          <desc version="2008-01-31" xml:lang="ja" >
          一般的な補語．
          </desc>

        </valItem>
        <valItem ident="obj">
          <gloss>object</gloss>

          <desc version="2008-01-31" xml:lang="ja" >
          一般的な目的語．
          </desc>

        </valItem>
        <valItem ident="subj">
          <gloss>subject</gloss>

          <desc version="2008-01-31" xml:lang="ja" >
          一般的な主語．
          </desc>

        </valItem>
        <valItem ident="verb">
          <equiv/>

          <desc version="2008-01-31" xml:lang="ja" >
          一般的な動詞．
          </desc>

        </valItem>
        <valItem ident="hint">
          <equiv/>


          <desc version="2008-01-31" xml:lang="ja" >
          分類できない，意味を選択するための情報，
          </desc>

        </valItem>
      </valList>
      <exemplum>
        <egXML xmlns="http://www.tei-c.org/ns/Examples"/>
      </exemplum>
      <remarks>
        <p/>
      </remarks>
    </attDef>
  </attList>
  <exemplum>
    <egXML xmlns="http://www.tei-c.org/ns/Examples">
      <form>
        <orth>colour</orth>
        <form>
          <usg type="geo">U.S.</usg>
          <orth>color</orth>
        </form>
      </form>
    </egXML>
  </exemplum>
  <remarks>
    <p/>
  </remarks>
  <listRef>
    <ptr target="#DITPUS"/>
  </listRef>
</elementSpec>
