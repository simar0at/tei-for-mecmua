      <specGrp xml:id="NEW">
        <elementSpec ident="object" module="namesdates" mode="add">
          <desc>contains data about an identifiable object</desc>
          <classes>
            <memberOf key="att.global"/>
            <memberOf key="att.typed"/>
            <memberOf key="att.declaring"/>
            <memberOf key="att.sortable"/>
            <memberOf key="model.objectLike"/>
          </classes>
          <content>
            <rng:group>
              <rng:zeroOrMore>
                <rng:ref name="model.headLike"/>
              </rng:zeroOrMore>
              <rng:choice>
                <rng:group>
                  <rng:zeroOrMore>
                    <rng:ref name="model.pLike"/>
                  </rng:zeroOrMore>
                </rng:group>
                <rng:zeroOrMore>
                  <rng:choice>
                    <rng:ref name="model.labelLike"/>
                    <rng:ref name="model.objectStateLike"/>
                    <rng:ref name="model.objectEventLike"/>
                  </rng:choice>
                </rng:zeroOrMore>
              </rng:choice>
              <rng:zeroOrMore>
                <rng:choice>
                  <rng:ref name="model.noteLike"/>
                  <rng:ref name="model.biblLike"/>
                </rng:choice>
              </rng:zeroOrMore>
              <rng:zeroOrMore>
                <rng:choice>
                  <rng:ref name="model.objectLike"/>
                  <rng:ref name="listObject"/>
                </rng:choice>
              </rng:zeroOrMore>
            </rng:group>
          </content>
        </elementSpec>
        <elementSpec module="namesdates" ident="listObject"  mode="add">
          <gloss>list of objects</gloss>
          <classes>
            <memberOf key="att.global"/>
            <memberOf key="att.typed"/>
            <memberOf key="att.declarable"/>
            <memberOf key="att.sortable"/>
            <memberOf key="model.listLike"/>
            <memberOf key="model.objectLike"/>
          </classes>
          <content>
            <rng:group>
              <rng:zeroOrMore>
                <rng:ref name="model.headLike"/>
              </rng:zeroOrMore>
              <rng:oneOrMore>
                <rng:choice>
                  <rng:ref name="model.objectLike"/>
                  <rng:ref name="listObject"/>
                </rng:choice>
              </rng:oneOrMore>
              <rng:zeroOrMore>
                <rng:choice>
                  <rng:ref name="listRelation"/>
                  <rng:ref name="relationGrp"/>
                  <rng:ref name="relation"/>
                </rng:choice>
              </rng:zeroOrMore>
            </rng:group>
          </content>
        </elementSpec>
        <elementSpec module="namesdates" ident="objectName" mode="add">
          <desc>contains the name of an identifiable object. </desc>
          <classes>
            <memberOf key="att.datable"/>
            <memberOf key="att.editLike"/>
            <memberOf key="att.global"/>
            <memberOf key="att.naming"/>
            <memberOf key="att.typed"/>
	    <memberOf key="model.nameLike"/>
          </classes>
          <content>
            <ref xmlns="http://relaxng.org/ns/structure/1.0" name="macro.phraseSeq"/>
          </content>
        </elementSpec>
        <classSpec ident="model.objectLike" type="model" module="namesdates" mode="add">
          <desc>groups elements used to provide information about
	   identifiable objects.</desc>
        </classSpec>
        <classSpec ident="model.objectStateLike" type="model" module="namesdates" mode="add">
          <desc>groups elements used to provide properties of identifiable objects.</desc>
        </classSpec>
        <classSpec ident="model.objectEventLike" type="model"
		   module="namesdates"  mode="add">
          <desc>groups elements used to provide information about
	   events relating to identifiable objects.</desc>
        </classSpec>
      </specGrp>
      <specGrp xml:id="CHANGES">
       </specGrp>
      <schemaSpec ident="object" start="TEI">
        <moduleRef key="tei"/>
        <moduleRef key="textstructure"/>
        <moduleRef key="core"/>
        <moduleRef key="header"/>
        <moduleRef key="linking"/>
        <moduleRef key="namesdates"/>
      </schemaSpec>
    </body>
