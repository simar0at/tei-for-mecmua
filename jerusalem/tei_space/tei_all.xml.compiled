      <desc version="2006-10-28" xml:lang="ja">当該再指定の対象となる削除について示す．</desc>
      <datatype>
        <rng:ref name="data.name"/>
      </datatype>
      <valDesc/>
      <exemplum>
        <egXML xmlns="http://www.tei-c.org/ns/Examples"/>
      </exemplum>
    </attDef>
              <attDef ident="hand" usage="rec">
