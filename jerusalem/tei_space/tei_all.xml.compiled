        <classSpec module="tei" xml:id="STAGEDIR" type="model" ident="model.stageLike" predeclare="true">
          <gloss xmlns:tei="http://www.tei-c.org/ns/1.0">stage
directions</gloss>
          <gloss xmlns:tei="http://www.tei-c.org/ns/1.0" version="2007-05-04" xml:lang="es">indicaciones de escena</gloss>
          <gloss xmlns:tei="http://www.tei-c.org/ns/1.0" version="2007-01-21" xml:lang="it">indicazioni di scena</gloss>
