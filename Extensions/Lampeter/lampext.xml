  <classes><memberOf key="tei.phrase"/></classes>
  <content>
    <rng:ref name="macro.phraseSeq"/>
  </content>
 <desc></desc>
</elementSpec>
 <elementSpec ident="bo">
  <classes><memberOf key="tei.phrase"/></classes>
