</module>
<module ident="corpus" mode="change">
  <elementSpec ident="preparedness" mode="delete"/>
  <elementSpec ident="purpose" mode="delete"/>
  <elementSpec ident="settingDesc" mode="delete"/>
  <elementSpec ident="textDesc" mode="delete"/>
  <elementSpec ident="activity" mode="delete"/>
  <elementSpec ident="locale" mode="delete"/>
  <elementSpec ident="relation" mode="delete"/>
  <elementSpec ident="setting" mode="delete"/>
  <elementSpec ident="firstLang" mode="delete"/>
  <elementSpec ident="interaction" mode="delete"/>
  <elementSpec ident="langKnown" mode="delete"/>
