<elementSpec ident="align">
  <desc>marks an temporal alignment point within transcribed speech</desc>
  <classes>
    <memberOf key="model.ptrLike"/>
  </classes>
  <content>
    <rng:empty/>
  </content>
  <attList>
    <attDef ident="with" usage="req">
      <equiv/>
      <desc>supplies an arbitrary identifier; all
      elements specifying the same value for this attribute are
      understood to be aligned with each other in time.</desc>
      <datatype>
	<rng:ref xmlns:rng="http://relaxng.org/ns/structure/1.0" name="data.pointer"/>
      </datatype>
      <valDesc>A unique identifier in the form....</valDesc>
    </attDef>
  </attList>
  <exemplum><egXML xmlns="http://www.tei-c.org/ns/Examples"><![CDATA[<u who="PS6SF">
   <s n="12">
        <w c5="VVB" hw="tell" pos="VERB">tell </w>
        <w c5="NP0" hw="billy" pos="SUBST">Billy </w>
        <align with="KSWLC001"/>
        <w c5="DT0" hw="that" pos="ADJ">that </w>
    </s></u>
<u who="PS6SJ">
  <s n="13">
       <align with="KSWLC001"/>
       <w c5="ITJ" hw="hello" pos="INTERJ">Hello </w>
       <w c5="AV0" hw="now" pos="ADV">now </w>
       <w c5="VM0" hw="can" pos="VERB">can </w>
       <w c5="PNP" hw="you" pos="PRON">you </w>
       <w c5="VVI" hw="hear" pos="VERB">hear </w>
       <w c5="PNP" hw="i" pos="PRON">me</w>
  </s></u>  ]]></egXML></exemplum>
</elementSpec>

