<rng:zeroOrMore>
  <rng:choice>
    <rng:text/>
    <rng:ref name="hi"/>
    <rng:ref name="list"/>
  </rng:choice>
</rng:zeroOrMore>
</content>
<exemplum><egXML xmlns="http://www.tei-c.org/ns/Examples"><para>For information, the conditions of the Standard License Agreement are as
follows:</para>
</egXML></exemplum></elementSpec>


