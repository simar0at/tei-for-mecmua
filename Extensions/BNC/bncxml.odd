<elementSpec ident="w" module="analysis" mode="change">
<classes>
<memberOf key="att.c5coded"/>
<memberOf key="model.segLike"/>
</classes>
<content>
<rng:text/>
</content>
<attList>
<attDef ident="pos" usage="req" mode="add">
      <equiv/>
      <desc>supplies a simplified  part-of-speech code.</desc>
      <valList type="closed">
<valItem ident="ADJ"><desc>adjective</desc></valItem>
<valItem ident="ADV"><desc>adverb</desc></valItem>
<valItem ident="ART"><desc>article</desc></valItem>
<valItem ident="CONJ"><desc>conjunction</desc></valItem>
<valItem ident="INTERJ"><desc>interjection</desc></valItem>
<valItem ident="PREP"><desc>preposition</desc></valItem>
<valItem ident="PRON"><desc>pronoun</desc></valItem>
<valItem ident="STOP"><desc>punctuation</desc></valItem>
<valItem ident="SUBST"><desc>substantive</desc></valItem>
<valItem ident="UNC"><desc>unclassified or non-lexical word</desc></valItem>
<valItem ident="VERB"><desc>verb</desc></valItem>
<!--valItem ident="XXX"><desc>unspecified</desc></valItem-->
      </valList>
    </attDef>
<attDef ident="lemma" mode="delete" />
<attDef ident="hw" usage="req">
<!--altIdent>hw</altIdent-->
      <desc>specifies the headword under which this lexical unit is
      conventionally grouped, where known.</desc>
<datatype><rng:text/></datatype>
      <valDesc>a lower-case only normalised form of a word</valDesc>
</attDef>
</attList>
<exemplum><egXML xmlns="http://www.tei-c.org/ns/Examples"><w c5="PNP" hw="i" pos="PRON">I </w><w c5="VDB" hw="do" pos="VERB">do</w><w c5="XX0" hw="not" pos="ADV">n't </w><w c5="VVI" hw="care" pos="VERB">care </w></egXML></exemplum></elementSpec>


